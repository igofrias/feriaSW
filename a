[32m+[m[32mimport java.util.Set;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport android.app.Activity;[m[41m[m
[32m+[m[32mimport android.bluetooth.BluetoothAdapter;[m[41m[m
[32m+[m[32mimport android.bluetooth.BluetoothDevice;[m[41m[m
[32m+[m[32mimport android.content.BroadcastReceiver;[m[41m[m
[32m+[m[32mimport android.content.Context;[m[41m[m
[32m+[m[32mimport android.content.Intent;[m[41m[m
[32m+[m[32mimport android.content.IntentFilter;[m[41m[m
[32m+[m[32mimport android.os.Bundle;[m[41m[m
[32m+[m[32mimport android.view.View;[m[41m[m

[31m-                    getFirmwareVersion();[m
[31m-                    break;[m
[31m-                case DISCONNECT:[m
[31m-                    // send stop messages before closing[m
[31m-                    changeMotorSpeed(MOTOR_A, 0);[m
[31m-                    changeMotorSpeed(MOTOR_B, 0);[m
[31m-                    changeMotorSpeed(MOTOR_C, 0);[m
[31m-                    waitSomeTime(500);[m
[31m-                    try {[m
[31m-                        destroyNXTconnection();[m
[31m-                    }[m
[31m-                    catch (IOException e) { }[m

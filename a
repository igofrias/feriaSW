[32m+[m[32m        Log.d("Reading: ", "Reading all contacts..");[m[41m[m
[32m+[m[32m        List<Pet> mascotas = db.getPets();[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        for (Pet cn : mascotas) {[m[41m[m
[32m+[m[32m            String log = "Id: "+cn.get_id()+", Name: " + cn.get_name() + ", Score: " + cn.get_raza();[m[41m[m
[32m+[m[32m                // Writing Contacts to log[m[41m[m
[32m+[m[32m        Log.d("Name: ", log);[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        db.close();[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        }[m[41m[m

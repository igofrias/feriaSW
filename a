[32m+[m	[32m//Confirma Realizacin de achievement.[m[41m[m
[32m+[m	[32m//Cambios esperados:[m[41m [m
[32m+[m	[32m//Inicial: Hecho = 0[m[41m[m
[32m+[m	[32m//Final: Hecho = 1[m[41m[m
[32m+[m	[32mpublic void confirmAchievement(int id, String name, String desc, int done){[m[41m[m
[32m+[m	[32m    ContentValues cv = new ContentValues();[m[41m[m
[32m+[m	[32m    cv.put("_id", id);[m[41m[m
[32m+[m		[32mcv.put(Key_name_ach, name);[m[41m[m
[32m+[m		[32mcv.put(Key_desc_ach, desc);[m[41m[m
[32m+[m		[32mcv.put(Key_done, done);[m[41m[m
[32m+[m	[32m    Database.update(DB_table_ach, cv, "_id=" + id, null);[m[41m   [m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m//Modifica Estadsticas Actuales[m[41m[m

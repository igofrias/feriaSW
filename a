[32m+[m[32mpublic class LCPMessage {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // the folowing constants were taken from the leJOS project (http://www.lejos.org)[m[41m [m
[32m+[m[41m    [m
[32m+[m[32m    // Command types constants. Indicates type of packet being sent or received.[m[41m[m
[32m+[m[32m    public static byte DIRECT_COMMAND_REPLY = 0x00;[m[41m[m
[32m+[m[32m    public static byte SYSTEM_COMMAND_REPLY = 0x01;[m[41m[m
[32m+[m[32m    public static byte REPLY_COMMAND = 0x02;[m[41m[m
[32m+[m[32m    public static byte DIRECT_COMMAND_NOREPLY = (byte)0x80; // Avoids ~100ms latency[m[41m[m
[32m+[m[32m    public static byte SYSTEM_COMMAND_NOREPLY = (byte)0x81; // Avoids ~100ms latency[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // Direct Commands[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public static final byte START_PROGRAM = 0x00;[m[41m[m
[32m+[m[32m    public static final byte STOP_PROGRAM = 0x01;[m[41m  [m
[32m+[m[32m    public static final byte PLAY_SOUND_FILE = 0x02;[m[41m[m
[32m+[m[32m    public static final byte PLAY_TONE = 0x03;[m[41m[m
[32m+[m[32m    public static final byte SET_OUTPUT_STATE = 0x04;[m[41m[m
[32m+[m[32m    public static final byte SET_INPUT_MODE = 0x05;[m[41m[m
[32m+[m[32m    public static final byte GET_OUTPUT_STATE = 0x06;[m[41m[m
[32m+[m[32m    public static final byte GET_INPUT_VALUES = 0x07;[m[41m[m
[32m+[m[32m    public static final byte RESET_SCALED_INPUT_VALUE = 0x08;[m[41m[m
[32m+[m[32m    public static final byte MESSAGE_WRITE = 0x09;[m[41m[m
[32m+[m[32m    public static final byte RESET_MOTOR_POSITION = 0x0A;[m[41m   [m
[32m+[m[32m    public static final byte GET_BATTERY_LEVEL = 0x0B;[m[41m[m
[32m+[m[32m    public static final byte STOP_SOUND_PLAYBACK = 0x0C;[m[41m[m
[32m+[m[32m    public static final byte KEEP_ALIVE = 0x0D;[m[41m[m
[32m+[m[32m    public static final byte LS_GET_STATUS = 0x0E;[m[41m[m
[32m+[m[32m    public static final byte LS_WRITE = 0x0F;[m[41m[m
[32m+[m[32m    public static final byte LS_READ = 0x10;[m[41m[m
[32m+[m[32m    public static final byte GET_CURRENT_PROGRAM_NAME = 0x11;[m[41m[m
[32m+[m[32m    public static final byte MESSAGE_READ = 0x13;[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    // NXJ additions[m[41m[m
[32m+[m[32m    public static byte NXJ_DISCONNECT = 0x20;[m[41m [m
[32m+[m[32m    public static byte NXJ_DEFRAG = 0x21;[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    // MINDdroidConnector additions[m[41m[m
[32m+[m[32m    public static final byte SAY_TEXT = 0x30;[m[41m[m
[32m+[m[32m    public static final byte VIBRATE_PHONE = 0x31;[m[41m[m
[32m+[m[32m    public static final byte ACTION_BUTTON = 0x32;[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    // System Commands:[m[41m[m
[32m+[m[32m    public static final byte OPEN_READ = (byte)0x80;[m[41m[m
[32m+[m[32m    public static final byte OPEN_WRITE = (byte)0x81;[m[41m[m
[32m+[m[32m    public static final byte READ = (byte)0x82;[m[41m[m
[32m+[m[32m    public static final byte WRITE = (byte)0x83;[m[41m[m
[32m+[m[32m    public static final byte CLOSE = (byte)0x84;[m[41m[m
[32m+[m[32m    public static final byte DELETE = (byte)0x85;[m[41m        [m
[32m+[m[32m    public static final byte FIND_FIRST = (byte)0x86;[m[41m[m
[32m+[m[32m    public static final byte FIND_NEXT = (byte)0x87;[m[41m[m
[32m+[m[32m    public static final byte GET_FIRMWARE_VERSION = (byte)0x88;[m[41m[m
[32m+[m[32m    public static final byte OPEN_WRITE_LINEAR = (byte)0x89;[m[41m[m
[32m+[m[32m    public static final byte OPEN_READ_LINEAR = (byte)0x8A;[m[41m[m
[32m+[m[32m    public static final byte OPEN_WRITE_DATA = (byte)0x8B;[m[41m[m
[32m+[m[32m    public static final byte OPEN_APPEND_DATA = (byte)0x8C;[m[41m[m
[32m+[m[32m    public static final byte BOOT = (byte)0x97;[m[41m[m
[32m+[m[32m    public static final byte SET_BRICK_NAME = (byte)0x98;[m[41m[m
[32m+[m[32m    public static final byte GET_DEVICE_INFO = (byte)0x9B;[m[41m[m
[32m+[m[32m    public static final byte DELETE_USER_FLASH = (byte)0xA0;[m[41m[m
[32m+[m[32m    public static final byte POLL_LENGTH = (byte)0xA1;[m[41m[m
[32m+[m[32m    public static final byte POLL = (byte)0xA2;[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public static final byte NXJ_FIND_FIRST = (byte)0xB6;[m[41m[m
[32m+[m[32m    public static final byte NXJ_FIND_NEXT = (byte)0xB7;[m[41m[m
[32m+[m[32m    public static final byte NXJ_PACKET_MODE = (byte)0xff;[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    // Error codes[m[41m    [m
[32m+[m[32m    public static final byte MAILBOX_EMPTY = (byte)0x40;[m[41m[m
[32m+[m[32m    public static final byte FILE_NOT_FOUND = (byte)0x86;[m[41m[m
[32m+[m[32m    public static final byte INSUFFICIENT_MEMORY = (byte) 0xFB;[m[41m[m
[32m+[m[32m    public static final byte DIRECTORY_FULL = (byte) 0xFC;[m[41m[m
[32m+[m[32m    public static final byte UNDEFINED_ERROR = (byte) 0x8A;[m[41m[m
[32m+[m[32m    public static final byte NOT_IMPLEMENTED = (byte) 0xFD;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // Firmware codes[m[41m[m
[32m+[m[32m    public static byte[] FIRMWARE_VERSION_LEJOSMINDDROID = { 0x6c, 0x4d, 0x49, 0x64 };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getBeepMessage(int frequency, int duration) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[6];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_NOREPLY;[m[41m[m
[32m+[m[32m        message[1] = PLAY_TONE;[m[41m[m
[32m+[m[32m        // Frequency for the tone, Hz (UWORD); Range: 200-14000 Hz[m[41m[m
[32m+[m[32m        message[2] = (byte) frequency;[m[41m[m
[32m+[m[32m        message[3] = (byte) (frequency >> 8);[m[41m[m
[32m+[m[32m        // Duration of the tone, ms (UWORD)[m[41m[m
[32m+[m[32m        message[4] = (byte) duration;[m[41m[m
[32m+[m[32m        message[5] = (byte) (duration >> 8);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public static byte[] getActionMessage(int actionNr) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[3];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_NOREPLY;[m[41m[m
[32m+[m[32m        message[1] = ACTION_BUTTON;[m[41m[m
[32m+[m[32m        message[2] = (byte) actionNr;[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m   [m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getMotorMessage(int motor, int speed) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[12];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_NOREPLY;[m[41m[m
[32m+[m[32m        message[1] = SET_OUTPUT_STATE;[m[41m[m
[32m+[m[32m        // Output port[m[41m[m
[32m+[m[32m        message[2] = (byte) motor;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (speed == 0) {[m[41m[m
[32m+[m[32m            message[3] = 0;[m[41m[m
[32m+[m[32m            message[4] = 0;[m[41m[m
[32m+[m[32m            message[5] = 0;[m[41m[m
[32m+[m[32m            message[6] = 0;[m[41m[m
[32m+[m[32m            message[7] = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } else {[m[41m[m
[32m+[m[32m            // Power set option (Range: -100 - 100)[m[41m[m
[32m+[m[32m            message[3] = (byte) speed;[m[41m[m
[32m+[m[32m            // Mode byte (Bit-field): MOTORON + BREAK[m[41m[m
[32m+[m[32m            message[4] = 0x03;[m[41m[m
[32m+[m[32m            // Regulation mode: REGULATION_MODE_MOTOR_SPEED[m[41m[m
[32m+[m[32m            message[5] = 0x01;[m[41m[m
[32m+[m[32m            // Turn Ratio (SBYTE; -100 - 100)[m[41m[m
[32m+[m[32m            message[6] = 0x00;[m[41m[m
[32m+[m[32m            // RunState: MOTOR_RUN_STATE_RUNNING[m[41m[m
[32m+[m[32m            message[7] = 0x20;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // TachoLimit: run forever[m[41m[m
[32m+[m[32m        message[8] = 0;[m[41m[m
[32m+[m[32m        message[9] = 0;[m[41m[m
[32m+[m[32m        message[10] = 0;[m[41m[m
[32m+[m[32m        message[11] = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getMotorMessage(int motor, int speed, int end) {[m[41m[m
[32m+[m[32m        byte[] message = getMotorMessage(motor, speed);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // TachoLimit[m[41m[m
[32m+[m[32m        message[8] = (byte) end;[m[41m[m
[32m+[m[32m        message[9] = (byte) (end >> 8);[m[41m[m
[32m+[m[32m        message[10] = (byte) (end >> 16);[m[41m[m
[32m+[m[32m        message[11] = (byte) (end >> 24);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getResetMessage(int motor) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[4];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_NOREPLY;[m[41m[m
[32m+[m[32m        message[1] = RESET_MOTOR_POSITION;[m[41m[m
[32m+[m[32m        // Output port[m[41m[m
[32m+[m[32m        message[2] = (byte) motor;[m[41m[m
[32m+[m[32m        // absolute position[m[41m[m
[32m+[m[32m        message[3] = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getOutputStateMessage(int motor) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[3];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = GET_OUTPUT_STATE;[m[41m[m
[32m+[m[32m        // Output port[m[41m[m
[32m+[m[32m        message[2] = (byte) motor;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getFirmwareVersionMessage() {[m[41m[m
[32m+[m[32m        byte[] message = new byte[2];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = SYSTEM_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = GET_FIRMWARE_VERSION;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public static byte[] getStartProgramMessage(String programName) {[m[41m[m
[32m+[m[41m    [m	[32mLog.d("pepe", "start program message");[m[41m[m
[32m+[m[32m        byte[] message = new byte[22];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_NOREPLY;[m[41m[m
[32m+[m[32m        message[1] = START_PROGRAM;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // copy programName and end with 0 delimiter[m[41m[m
[32m+[m[32m        for (int pos=0; pos<programName.length(); pos++)[m[41m[m
[32m+[m[32m            message[2+pos] = (byte) programName.charAt(pos);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[programName.length()+2] = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getStopProgramMessage() {[m[41m[m
[32m+[m[32m        byte[] message = new byte[2];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_NOREPLY;[m[41m[m
[32m+[m[32m        message[1] = STOP_PROGRAM;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public static byte[] getProgramNameMessage() {[m[41m[m
[32m+[m[41m    [m	[32mLog.d("pepe", "got name del lcp >:C");[m[41m[m
[32m+[m[32m        byte[] message = new byte[2];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = DIRECT_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = GET_CURRENT_PROGRAM_NAME;[m[41m[m
[32m+[m[32m        Log.d("pepe", "got name del lcp >:C fin");[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getOpenWriteMessage(String fileName, int fileLength) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[26];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = SYSTEM_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = OPEN_WRITE;[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        // copy programName and end with 0 delimiter[m[41m[m
[32m+[m[32m        for (int pos=0; pos<fileName.length(); pos++)[m[41m[m
[32m+[m[32m            message[2+pos] = (byte) fileName.charAt(pos);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[fileName.length()+2] = 0;[m[41m[m
[32m+[m[32m        // copy file size[m[41m[m
[32m+[m[32m        message[22] = (byte) fileLength;[m[41m[m
[32m+[m[32m        message[23] = (byte) (fileLength >>> 8);[m[41m[m
[32m+[m[32m        message[24] = (byte) (fileLength >>> 16);[m[41m[m
[32m+[m[32m        message[25] = (byte) (fileLength >>> 24);[m[41m        [m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getDeleteMessage(String fileName) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[22];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = SYSTEM_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = DELETE;[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        // copy programName and end with 0 delimiter[m[41m[m
[32m+[m[32m        for (int pos=0; pos<fileName.length(); pos++)[m[41m[m
[32m+[m[32m            message[2+pos] = (byte) fileName.charAt(pos);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[fileName.length()+2] = 0;[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static byte[] getWriteMessage(int handle, byte[] data, int dataLength) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[dataLength + 3];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = SYSTEM_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = WRITE;[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        // copy handle[m[41m[m
[32m+[m[32m        message[2] = (byte) handle;[m[41m[m
[32m+[m[32m        // copy data[m[41m[m
[32m+[m[32m        System.arraycopy(data, 0, message, 3, dataLength);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public static byte[] getCloseMessage(int handle) {[m[41m[m
[32m+[m[32m        byte[] message = new byte[3];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        message[0] = SYSTEM_COMMAND_REPLY;[m[41m[m
[32m+[m[32m        message[1] = CLOSE;[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        // copy handle[m[41m[m
[32m+[m[32m        message[2] = (byte) handle;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return message;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m}[m[41m[m

[32m+[m		[32msuper.onCreate(savedInstanceState);[m[41m[m
[32m+[m		[32mmanager =(SensorManager) getSystemService(Context.SENSOR_SERVICE);[m[41m[m
[32m+[m		[32macelerometro = manager.getDefaultSensor(Sensor.TYPE_ACCELEROMETER);[m[41m[m
[32m+[m		[32mgravity = new double[3];[m[41m[m
[32m+[m		[32mMAX_RANGE = 127;[m[41m[m
[32m+[m		[32mMAX_VEL = 22;[m[41m[m
[32m+[m	[32m}[m[41m	[m
[32m+[m[41m		[m
[32m+[m[41m	 [m
[32m+[m	[32mpublic void procesar_aceleraciones()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Ocupa los filtros indicados en el tutorial para dejar una aceleracion[m[41m[m
[32m+[m		[32m//pura sin influencia de la gravedad.[m[41m[m
[32m+[m		[32m//alpha  = t/(dt + t) t=constante del filtro dt=event delivery rate[m[41m[m
[32m+[m		[32mdouble alpha = 0.1;[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mfor (int i = 0; i < 3; i++)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mgravity[i] = 0;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32m  // Isolate the force of gravity with the low-pass filter.[m[41m[m
[32m+[m		[32m  gravity[0] = alpha * gravity[0] + (1 - alpha) * raw_accel_x;[m[41m[m
[32m+[m		[32m  gravity[1] = alpha * gravity[1] + (1 - alpha) * raw_accel_y;[m[41m[m
[32m+[m		[32m  gravity[2] = alpha * gravity[2] + (1 - alpha) * raw_accel_z;[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m  // Remove the gravity contribution with the high-pass filter.[m[41m[m
[32m+[m		[32m  accel_x = raw_accel_x - gravity[0];[m[41m[m
[32m+[m		[32m  accel_y = raw_accel_y - gravity[1];[m[41m[m
[32m+[m		[32m  accel_z = raw_accel_z - gravity[2];[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic int trans_vel_a_robot(double vel)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Transforma una velocidad de celular a perro[m[41m[m
[32m+[m		[32m//vel_robot=MAX_RANGE*vel/MAX_VEL[m[41m[m
[32m+[m		[32mint k = (int)(Math.ceil((double)MAX_RANGE/MAX_VEL));[m[41m[m
[32m+[m		[32mint vel_robot = (int) (Math.ceil(vel)*k);[m[41m[m
[32m+[m		[32mreturn vel_robot;[m[41m[m
[32m+[m[41m				[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void obtener_velocidades()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//A partir de la aceleracion obtiene la velocidad a mover[m[41m[m
[32m+[m		[32mvel_x = vel_x + accel_x;[m[41m[m
[32m+[m		[32mvel_y = vel_y + accel_y;[m[41m[m
[32m+[m		[32mvel_z = vel_z + accel_z;[m[41m[m
[32m+[m[41m		[m
[32m+[m				[32m//vel_robot=MAX_RANGE*vel/MAX_VEL[m[41m[m
[32m+[m		[32mvel_robot_x = this.trans_vel_a_robot(vel_x);[m[41m[m
[32m+[m		[32mvel_robot_y = this.trans_vel_a_robot(vel_y);[m[41m[m
[32m+[m		[32mvel_robot_z = this.trans_vel_a_robot(vel_z);[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic final void onAccuracyChanged(Sensor sensor, int accuracy) {[m[41m[m
[32m+[m	[32m    // Do something here if sensor accuracy changes.[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic final void onSensorChanged(SensorEvent event) {[m[41m[m
[32m+[m		[32mif (event.sensor.getType() != Sensor.TYPE_ACCELEROMETER)[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m	[32m    //Cada vez que cambia el sensor se reciben los valores y se procesan.[m[41m[m
[32m+[m	[32m    raw_accel_x = event.values[0];[m[41m[m
[32m+[m	[32m    raw_accel_y = event.values[1];[m[41m[m
[32m+[m	[32m    raw_accel_z = event.values[2];[m[41m[m
[32m+[m	[32m    // Do something with this sensor value.[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mprotected void onResume() {[m[41m[m
[32m+[m		[32msuper.onResume();[m[41m[m
[32m+[m		[32mmanager.registerListener(this, acelerometro, SensorManager.SENSOR_DELAY_NORMAL);[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mprotected void onPause() {[m[41m[m
[32m+[m		[32msuper.onPause();[m[41m[m
[32m+[m		[32mmanager.unregisterListener(this);[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic int getMAX_RANGE() {[m[41m[m
[32m+[m		[32mreturn MAX_RANGE;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void setMAX_RANGE(int mAX_RANGE) {[m[41m[m
[32m+[m		[32mMAX_RANGE = mAX_RANGE;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic double getMAX_VEL() {[m[41m[m
[32m+[m		[32mreturn MAX_VEL;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void setMAX_VEL(double mAX_VEL) {[m[41m[m
[32m+[m		[32mMAX_VEL = mAX_VEL;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void setAcelerometro(Sensor acelerometro) {[m[41m[m
[32m+[m		[32mthis.acelerometro = acelerometro;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void setManager(SensorManager manager) {[m[41m[m
[32m+[m		[32mthis.manager = manager;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m[m

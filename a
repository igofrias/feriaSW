[31m-        }[m
[31m-    };[m
[31m-}[m
[32m+[m[32mpackage com.Phyrex.VIPeR;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/**[m[41m[m
[32m+[m[32m *   (Changes from original are) Copyright 2010 Guenther Hoelzl, Shawn Brown[m[41m[m
[32m+[m[32m *[m[41m [m
[32m+[m[32m * (original work is) Copyright (C) 2009 The Android Open Source Project[m[41m[m
[32m+[m[32m**/[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport android.bluetooth.BluetoothAdapter;[m[41m[m
[32m+[m[32mimport android.bluetooth.BluetoothDevice;[m[41m[m
[32m+[m[32mimport android.bluetooth.BluetoothSocket;[m[41m[m
[32m+[m[32mimport android.content.res.Resources;[m[41m[m
[32m+[m[32mimport android.os.Bundle;[m[41m[m
[32m+[m[32mimport android.os.Handler;[m[41m[m
[32m+[m[32mimport android.os.Message;[m[41m[m
[32m+[m[32mimport android.util.Log;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport java.io.IOException;[m[41m[m
[32m+[m[32mimport java.io.InputStream;[m[41m[m
[32m+[m[32mimport java.io.OutputStream;[m[41m[m
[32m+[m[32mimport java.lang.reflect.Method;[m[41m[m
[32m+[m[32mimport java.util.UUID;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/**[m[41m[m
[32m+[m[32m * This class is for talking to a LEGO NXT robot via bluetooth.[m[41m[m
[32m+[m[32m * The communciation to the robot is done via LCP (LEGO communication protocol).[m[41m[m
[32m+[m[32m * Objects of this class can either be run as standalone thread or controlled[m[41m[m
[32m+[m[32m * by the owners, i.e. calling the send/receive methods by themselves.[m[41m[m
[32m+[m[32m */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m//Nos servira manejo de un motor, desconectar, estados de conexin y errores.[m[41m[m
[32m+[m[32mpublic class BTCommunicator extends Thread {[m[41m[m
[32m+[m[32m    public static final int MOTOR_A = 0;[m[41m[m
[32m+[m[32m    public static final int MOTOR_B = 1;[m[41m[m
[32m+[m[32m    public static final int MOTOR_C = 2;[m[41m[m
[32m+[m[32m    public static final int MOTOR_B_ACTION = 40;[m[41m[m
[32m+[m[32m    public static final int MOTOR_RESET = 10;[m[41m[m
[32m+[m[32m    public static final int DO_BEEP = 51;[m[41m[m
[32m+[m[32m    public static final int DO_ACTION = 52;[m[41m    [m
[32m+[m[32m    public static final int READ_MOTOR_STATE = 60;[m[41m[m
[32m+[m[32m    public static final int GET_FIRMWARE_VERSION = 70;[m[41m[m
[32m+[m[32m    public static final int DISCONNECT = 99;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static final int DISPLAY_TOAST = 1000;[m[41m[m
[32m+[m[32m    public static final int STATE_CONNECTED = 1001;[m[41m[m
[32m+[m[32m    public static final int STATE_CONNECTERROR = 1002;[m[41m[m
[32m+[m[32m    public static final int STATE_CONNECTERROR_PAIRING = 1022;[m[41m[m
[32m+[m[32m    public static final int MOTOR_STATE = 1003;[m[41m[m
[32m+[m[32m    public static final int STATE_RECEIVEERROR = 1004;[m[41m[m
[32m+[m[32m    public static final int STATE_SENDERROR = 1005;[m[41m[m
[32m+[m[32m    public static final int FIRMWARE_VERSION = 1006;[m[41m[m
[32m+[m[32m    public static final int FIND_FILES = 1007;[m[41m[m
[32m+[m[32m    public static final int START_PROGRAM = 1008;[m[41m[m
[32m+[m[32m    public static final int STOP_PROGRAM = 1009;[m[41m[m
[32m+[m[32m    public static final int GET_PROGRAM_NAME = 1010;[m[41m[m
[32m+[m[32m    public static final int PROGRAM_NAME = 1011;[m[41m[m
[32m+[m[32m    public static final int SAY_TEXT = 1030;[m[41m[m
[32m+[m[32m    public static final int VIBRATE_PHONE = 1031;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public static final int NO_DELAY = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private static final UUID SERIAL_PORT_SERVICE_CLASS_UUID = UUID.fromString("00001101-0000-1000-8000-00805F9B34FB");[m[41m[m
[32m+[m[32m    // this is the only OUI registered by LEGO, see http://standards.ieee.org/regauth/oui/index.shtml[m[41m[m
[32m+[m[32m    public static final String OUI_LEGO = "00:16:53";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private Resources mResources;[m[41m[m
[32m+[m[32m    private BluetoothAdapter btAdapter;[m[41m[m
[32m+[m[32m    private BluetoothSocket nxtBTsocket = null;[m[41m[m
[32m+[m[32m    private OutputStream nxtOutputStream = null;[m[41m[m
[32m+[m[32m    private InputStream nxtInputStream = null;[m[41m[m
[32m+[m[32m    private boolean connected = false;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private Handler uiHandler;[m[41m[m
[32m+[m[32m    private String mMACaddress;[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mprivate BTConnectable myOwner;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private byte[] returnMessage;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public BTCommunicator(BTConnectable myOwner, Handler uiHandler, BluetoothAdapter btAdapter, Resources resources) {[m[41m[m
[32m+[m[32m        this.myOwner = myOwner;[m[41m[m
[32m+[m[32m        this.uiHandler = uiHandler;[m[41m[m
[32m+[m[32m        this.btAdapter = btAdapter;[m[41m[m
[32m+[m[32m        this.mResources = resources;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public Handler getHandler() {[m[41m[m
[32m+[m[32m        return myHandler;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public byte[] getReturnMessage() {[m[41m[m
[32m+[m[32m        return returnMessage;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public void setMACAddress(String mMACaddress) {[m[41m[m
[32m+[m[32m        this.mMACaddress = mMACaddress;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * @return The current status of the connection[m[41m[m
[32m+[m[32m     */[m[41m            [m
[32m+[m[32m    public boolean isConnected() {[m[41m[m
[32m+[m[32m        return connected;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Creates the connection, waits for incoming messages and dispatches them. The thread will be terminated[m[41m[m
[32m+[m[32m     * on closing of the connection.[m[41m[m
[32m+[m[32m     */[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    public void run() {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        try {[m[41m        [m
[32m+[m[32m            createNXTconnection();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (IOException e) { }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        while (connected) {[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                returnMessage = receiveMessage();[m[41m[m
[32m+[m[32m                if ((returnMessage.length >= 2) && ((returnMessage[0] == LCPMessage.REPLY_COMMAND) ||[m[41m[m
[32m+[m[32m                    (returnMessage[0] == LCPMessage.DIRECT_COMMAND_NOREPLY)))[m[41m[m
[32m+[m[32m                    dispatchMessage(returnMessage);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            } catch (IOException e) {[m[41m[m
[32m+[m[32m                // don't inform the user when connection is already closed[m[41m[m
[32m+[m[32m                if (connected)[m[41m[m
[32m+[m[32m                    sendState(STATE_RECEIVEERROR);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Create a bluetooth connection with SerialPortServiceClass_UUID[m[41m[m
[32m+[m[32m     * @see <a href=[m[41m[m
[32m+[m[32m     *      "http://lejos.sourceforge.net/forum/viewtopic.php?t=1991&highlight=android"[m[41m[m
[32m+[m[32m     *      />[m[41m[m
[32m+[m[32m     * On error the method either sends a message to it's owner or creates an exception in the[m[41m[m
[32m+[m[32m     * case of no message handler.[m[41m[m
[32m+[m[32m     */[m[41m[m
[32m+[m[32m    public void createNXTconnection() throws IOException {[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            BluetoothSocket nxtBTSocketTemporary;[m[41m[m
[32m+[m[32m            BluetoothDevice nxtDevice = null;[m[41m[m
[32m+[m[32m            nxtDevice = btAdapter.getRemoteDevice(mMACaddress);[m[41m[m
[32m+[m[32m            if (nxtDevice == null) {[m[41m[m
[32m+[m[32m                if (uiHandler == null)[m[41m[m
[32m+[m[32m                    throw new IOException();[m[41m[m
[32m+[m[32m                else {[m[41m[m
[32m+[m[32m                    sendToast(mResources.getString(R.string.no_paired_nxt));[m[41m[m
[32m+[m[32m                    sendState(STATE_CONNECTERROR);[m[41m[m
[32m+[m[32m                    return;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            nxtBTSocketTemporary = nxtDevice.createRfcommSocketToServiceRecord(SERIAL_PORT_SERVICE_CLASS_UUID);[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                nxtBTSocketTemporary.connect();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (IOException e) {[m[41m  [m
[32m+[m[32m                if (myOwner.isPairing()) {[m[41m[m
[32m+[m[32m                    if (uiHandler != null) {[m[41m[m
[32m+[m[32m                        sendToast(mResources.getString(R.string.pairing_message));[m[41m[m
[32m+[m[32m                        sendState(STATE_CONNECTERROR_PAIRING);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        throw e;[m[41m[m
[32m+[m[32m                    return;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // try another method for connection, this should work on the HTC desire, credits to Michael Biermann[m[41m[m
[32m+[m[32m                try {[m[41m[m
[32m+[m[32m                    Method mMethod = nxtDevice.getClass().getMethod("createRfcommSocket", new Class[] { int.class });[m[41m[m
[32m+[m[32m                    nxtBTSocketTemporary = (BluetoothSocket) mMethod.invoke(nxtDevice, Integer.valueOf(1));[m[41m            [m
[32m+[m[32m                    nxtBTSocketTemporary.connect();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch (Exception e1){[m[41m[m
[32m+[m[32m                    if (uiHandler == null)[m[41m[m
[32m+[m[32m                        throw new IOException();[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        sendState(STATE_CONNECTERROR);[m[41m[m
[32m+[m[32m                    return;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            nxtBTsocket = nxtBTSocketTemporary;[m[41m[m
[32m+[m[32m            nxtInputStream = nxtBTsocket.getInputStream();[m[41m[m
[32m+[m[32m            nxtOutputStream = nxtBTsocket.getOutputStream();[m[41m[m
[32m+[m[32m            connected = true;[m[41m[m
[32m+[m[32m        } catch (IOException e) {[m[41m[m
[32m+[m[32m            if (uiHandler == null)[m[41m[m
[32m+[m[32m                throw e;[m[41m[m
[32m+[m[32m            else {[m[41m[m
[32m+[m[32m                if (myOwner.isPairing())[m[41m[m
[32m+[m[32m                    sendToast(mResources.getString(R.string.pairing_message));[m[41m[m
[32m+[m[32m                sendState(STATE_CONNECTERROR);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        // everything was OK[m[41m[m
[32m+[m[32m        if (uiHandler != null)[m[41m[m
[32m+[m[32m            sendState(STATE_CONNECTED);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Closes the bluetooth connection. On error the method either sends a message[m[41m[m
[32m+[m[32m     * to it's owner or creates an exception in the case of no message handler.[m[41m[m
[32m+[m[32m     */[m[41m[m
[32m+[m[32m    public void destroyNXTconnection() throws IOException {[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            if (nxtBTsocket != null) {[m[41m[m
[32m+[m[32m                connected = false;[m[41m[m
[32m+[m[32m                nxtBTsocket.close();[m[41m[m
[32m+[m[32m                nxtBTsocket = null;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            nxtInputStream = null;[m[41m[m
[32m+[m[32m            nxtOutputStream = null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } catch (IOException e) {[m[41m[m
[32m+[m[32m            if (uiHandler == null)[m[41m[m
[32m+[m[32m                throw e;[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                sendToast(mResources.getString(R.string.problem_at_closing));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Sends a message on the opened OutputStream[m[41m[m
[32m+[m[32m     * @param message, the message as a byte array[m[41m[m
[32m+[m[32m     */[m[41m[m
[32m+[m[32m    public void sendMessage(byte[] message) throws IOException {[m[41m[m
[32m+[m[41m    [m	[32mLog.d("pepe", "sendMessage");[m[41m[m
[32m+[m[32m        if (nxtOutputStream == null)[m[41m[m
[32m+[m[32m            throw new IOException();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // send message length[m[41m[m
[32m+[m[32m        int messageLength = message.length;[m[41m[m
[32m+[m[32m        nxtOutputStream.write(messageLength);[m[41m[m
[32m+[m[32m        nxtOutputStream.write(messageLength >> 8);[m[41m[m
[32m+[m[32m        nxtOutputStream.write(message, 0, message.length);[m[41m[m
[32m+[m[32m        Log.d("pepe", "sendMessage fin");[m[41m[m
[32m+[m[32m    }[m[41m  [m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Receives a message on the opened InputStream[m[41m[m
[32m+[m[32m     * @return the message[m[41m[m
[32m+[m[32m     */[m[41m                [m
[32m+[m[32m    public byte[] receiveMessage() throws IOException {[m[41m[m
[32m+[m[32m        if (nxtInputStream == null)[m[41m[m
[32m+[m[32m            throw new IOException();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        int length = nxtInputStream.read();[m[41m[m
[32m+[m[32m        length = (nxtInputStream.read() << 8) + length;[m[41m[m
[32m+[m[32m        byte[] returnMessage = new byte[length];[m[41m[m
[32m+[m[32m        nxtInputStream.read(returnMessage);[m[41m[m
[32m+[m[32m        return returnMessage;[m[41m[m
[32m+[m[32m    }[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Sends a message on the opened OutputStream. In case of[m[41m [m
[32m+[m[32m     * an error the state is sent to the handler.[m[41m[m
[32m+[m[32m     * @param message, the message as a byte array[m[41m[m
[32m+[m[32m     */[m[41m[m
[32m+[m[32m    private void sendMessageAndState(byte[] message) {[m[41m[m
[32m+[m[41m    [m	[32mLog.d("pepe", "sendMessageAndState");[m[41m[m
[32m+[m[32m        if (nxtOutputStream == null){[m[41m[m
[32m+[m[41m        [m	[32mLog.d("pepe", "sendMessageAndState");[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[41m        [m	[32mLog.d("pepe", "try send message  >:C");[m[41m[m
[32m+[m[32m            sendMessage(message);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        catch (IOException e) {[m[41m[m
[32m+[m[32m            sendState(STATE_SENDERROR);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void dispatchMessage(byte[] message) {[m[41m[m
[32m+[m[32m        switch (message[1]) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            case LCPMessage.GET_OUTPUT_STATE:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (message.length >= 25)[m[41m[m
[32m+[m[32m                    sendState(MOTOR_STATE);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            case LCPMessage.GET_FIRMWARE_VERSION:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (message.length >= 7)[m[41m[m
[32m+[m[32m                    sendState(FIRMWARE_VERSION);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                break;[m[41m[m
[32m+[m[41m                               [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void startProgram(String programName) {[m[41m[m
[32m+[m[41m    [m	[32mLog.d("pepe", "start");[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getStartProgramMessage(programName);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void stopProgram() {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getStopProgramMessage();[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    private void getProgramName() {[m[41m[m
[32m+[m[41m    [m	[32mLog.d("pepe", "got program name  >:C");[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getProgramNameMessage();[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m        Log.d("pepe", "got name del get program >:C");[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    private void doBeep(int frequency, int duration) {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getBeepMessage(frequency, duration);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m        waitSomeTime(20);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    private void doAction(int actionNr) {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getActionMessage(actionNr);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    private void changeMotorSpeed(int motor, int speed) {[m[41m[m
[32m+[m[32m        if (speed > 100)[m[41m[m
[32m+[m[32m            speed = 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        else if (speed < -100)[m[41m[m
[32m+[m[32m            speed = -100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getMotorMessage(motor, speed);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void rotateTo(int motor, int end) {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getMotorMessage(motor, -80, end);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m[m
[32m+[m[32m    private void reset(int motor) {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getResetMessage(motor);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void readMotorState(int motor) {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getOutputStateMessage(motor);[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void getFirmwareVersion() {[m[41m[m
[32m+[m[32m        byte[] message = LCPMessage.getFirmwareVersionMessage();[m[41m[m
[32m+[m[32m        sendMessageAndState(message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void waitSomeTime(int millis) {[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            Thread.sleep(millis);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        } catch (InterruptedException e) {[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void sendToast(String toastText) {[m[41m[m
[32m+[m[32m        Bundle myBundle = new Bundle();[m[41m[m
[32m+[m[32m        myBundle.putInt("message", DISPLAY_TOAST);[m[41m[m
[32m+[m[32m        myBundle.putString("toastText", toastText);[m[41m[m
[32m+[m[32m        sendBundle(myBundle);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void sendState(int message) {[m[41m[m
[32m+[m[32m        Bundle myBundle = new Bundle();[m[41m[m
[32m+[m[32m        myBundle.putInt("message", message);[m[41m[m
[32m+[m[32m        sendBundle(myBundle);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void sendBundle(Bundle myBundle) {[m[41m[m
[32m+[m[32m        Message myMessage = myHandler.obtainMessage();[m[41m[m
[32m+[m[32m        myMessage.setData(myBundle);[m[41m[m
[32m+[m[32m        uiHandler.sendMessage(myMessage);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // receive messages from the UI[m[41m[m
[32m+[m[32m    final Handler myHandler = new Handler() {[m[41m[m
[32m+[m[32m        @Override[m[41m[m
[32m+[m[32m        public void handleMessage(Message myMessage) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int message;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            switch (message = myMessage.getData().getInt("message")) {[m[41m[m
[32m+[m[32m                case MOTOR_A:[m[41m[m
[32m+[m[32m                case MOTOR_B:[m[41m[m
[32m+[m[32m                case MOTOR_C:[m[41m[m
[32m+[m[32m                    changeMotorSpeed(message, myMessage.getData().getInt("value1"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case MOTOR_B_ACTION:[m[41m[m
[32m+[m[32m                    rotateTo(MOTOR_B, myMessage.getData().getInt("value1"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[41m                    [m
[32m+[m[32m                case MOTOR_RESET:[m[41m[m
[32m+[m[32m                    reset(myMessage.getData().getInt("value1"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DO_BEEP:[m[41m[m
[32m+[m[32m                    doBeep(myMessage.getData().getInt("value1"), myMessage.getData().getInt("value2"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case START_PROGRAM:[m[41m[m
[32m+[m[41m                [m	[32mLog.d("pepe", "star program! :D >:C");[m[41m[m
[32m+[m[32m                    startProgram(myMessage.getData().getString("name"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case STOP_PROGRAM:[m[41m[m
[32m+[m[41m                [m	[32mstopProgram();[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case GET_PROGRAM_NAME:[m[41m[m
[32m+[m[41m                [m	[32mLog.d("pepe", "got name  >:C");[m[41m[m
[32m+[m[32m                    getProgramName();[m[41m[m
[32m+[m[32m                    Log.d("pepe", "got name  >:C fin");[m[41m[m
[32m+[m[32m                    break;[m[41m    [m
[32m+[m[32m                case DO_ACTION:[m[41m[m
[32m+[m[32m                    doAction(myMessage.getData().getInt("value1"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case READ_MOTOR_STATE:[m[41m[m
[32m+[m[32m                    readMotorState(myMessage.getData().getInt("value1"));[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case GET_FIRMWARE_VERSION:[m[41m[m
[32m+[m[32m                    getFirmwareVersion();[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DISCONNECT:[m[41m[m
[32m+[m[32m                    // send stop messages before closing[m[41m[m
[32m+[m[32m                    changeMotorSpeed(MOTOR_A, 0);[m[41m[m
[32m+[m[32m                    changeMotorSpeed(MOTOR_B, 0);[m[41m[m
[32m+[m[32m                    changeMotorSpeed(MOTOR_C, 0);[m[41m[m
[32m+[m[32m                    waitSomeTime(500);[m[41m[m
[32m+[m[32m                    try {[m[41m[m
[32m+[m[32m                        destroyNXTconnection();[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    catch (IOException e) { }[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[32m}[m[41m[m

[32m+[m	[32m}[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic void onAttach(Activity activity)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32msuper.onAttach(activity);[m[41m[m
[32m+[m		[32mparent_activity = activity;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32m  public View onCreateView(LayoutInflater inflater, ViewGroup container,[m[41m[m
[32m+[m	[32m      Bundle savedInstanceState) {[m[41m[m
[32m+[m		[32m  //Creates fragment[m[41m[m
[32m+[m		[32mcanvas = new DrawJoystick(getActivity());[m[41m[m
[32m+[m	[32m    //View view = inflater.inflate(R.layout.control_remoto_debug_layout,[m[41m[m
[32m+[m	[32m        //container, false);[m[41m[m
[32m+[m[41m	    [m
[32m+[m	[32m    manager =(SensorManager) getActivity().getSystemService(Context.SENSOR_SERVICE);[m[41m[m
[32m+[m		[32maccelerometer = manager.getDefaultSensor(Sensor.TYPE_ACCELEROMETER);[m[41m[m
[32m+[m		[32mgravity = new double[3];[m[41m[m
[32m+[m		[32mMAX_RANGE = 100;[m[41m[m
[32m+[m		[32mMAX_VEL = 10;[m[41m[m
[32m+[m		[32msens = 0.8;[m[41m[m
[32m+[m		[32mcanvas.setWillNotDraw(false);[m[41m[m
[32m+[m	[32m    return canvas;[m[41m[m
[32m+[m	[32m  }[m[41m	[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic void onActivityCreated(Bundle savedInstanceState) {[m[41m[m
[32m+[m		[32msuper.onActivityCreated(savedInstanceState);[m[41m[m
[32m+[m		[32mthisActivity = (MainActivity) getActivity();[m[41m[m
[32m+[m		[32mthMesseger = new Thread(new Runnable() {[m[41m[m
[32m+[m	[32m        public void run() {[m[41m[m
[32m+[m	[41m        [m	[32mlong wait_time = 100;[m[41m[m
[32m+[m	[41m        [m	[32mwhile(running)[m[41m[m
[32m+[m	[41m        [m	[32m{[m[41m[m
[32m+[m	[41m        [m		[32mif(thisActivity.isConnected())[m[41m[m
[32m+[m	[41m        [m		[32m{[m[41m[m
[32m+[m	[41m        [m			[32msend_speeds();[m[41m[m
[32m+[m[41m	        			[m
[32m+[m	[41m        [m			[32m//SurfaceHolder hold = canvas.getHolder();[m[41m[m
[32m+[m	[41m        [m			[32m//Canvas can = hold.lockCanvas();[m[41m[m
[32m+[m[41m	        			[m
[32m+[m	[41m        [m			[32m//	canvas.draw(can);[m[41m[m
[32m+[m[41m	       [m
[32m+[m	[41m        [m			[32m//hold.unlockCanvasAndPost(can);[m[41m[m
[32m+[m[41m	        			[m
[32m+[m	[41m        [m		[32m}[m[41m[m
[32m+[m	[41m        [m		[32mSystemClock.sleep(wait_time);[m[41m[m
[32m+[m[41m	        		[m
[32m+[m	[41m        [m	[32m}[m[41m[m
[32m+[m	[32m        }[m[41m[m
[32m+[m	[32m    });[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void process_aceleration()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Ocupa los filtros indicados en el tutorial para dejar una aceleracion[m[41m[m
[32m+[m		[32m//pura sin influencia de la gravedad.[m[41m[m
[32m+[m		[32m//alpha  = t/(dt + t) t=constante del filtro dt=event delivery rate[m[41m[m
[32m+[m		[32mdouble alpha = 0.8;[m[41m[m
[32m+[m		[32mfor (int i = 0; i < 3; i++)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mgravity[i] = 0;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32m  // Isolate the force of gravity with the low-pass filter.[m[41m[m
[32m+[m		[32m  gravity[0] = alpha * gravity[0] + (1 - alpha) * raw_accel_x;[m[41m[m
[32m+[m		[32m  gravity[1] = alpha * gravity[1] + (1 - alpha) * raw_accel_y;[m[41m[m
[32m+[m		[32m  gravity[2] = alpha * gravity[2] + (1 - alpha) * raw_accel_z;[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m  // Remove the gravity contribution with the high-pass filter.[m[41m[m
[32m+[m		[32m  accel_x = raw_accel_x - gravity[0];[m[41m[m
[32m+[m		[32m  accel_y = raw_accel_y - gravity[1];[m[41m[m
[32m+[m		[32m  accel_z = raw_accel_z - gravity[2];[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic int trans_vel_a_robot(double vel)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Transforma una velocidad de celular a perro[m[41m[m
[32m+[m		[32m//Velocidad del robot proporcional a rango de movimiento[m[41m [m
[32m+[m		[32mdouble k =(double)MAX_RANGE/MAX_VEL;[m[41m[m
[32m+[m		[32mint vel_robot = (int) (Math.ceil(vel*sens*k));[m[41m[m
[32m+[m		[32mif(vel_robot > MAX_RANGE)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_robot = MAX_RANGE;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32melse if (vel_robot < -MAX_RANGE)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_robot = -MAX_RANGE;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mreturn vel_robot;[m[41m[m
[32m+[m[41m				[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void speeds_to_robot()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Transforma los ejes x e y en velocidades por motor[m[41m[m
[32m+[m		[32m//Sacado del codigo de minddroids[m[41m[m
[32m+[m		[32mboolean direccion = true; //true == arriba acostado; false == derecho[m[41m[m
[32m+[m		[32mif (!direccion)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mdouble temp_x = vel_x;[m[41m[m
[32m+[m			[32mdouble temp_y = vel_y;[m[41m[m
[32m+[m			[32mvel_x = temp_y;[m[41m[m
[32m+[m			[32mvel_y = temp_x;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mif (vel_x > MAX_VEL){[m[41m[m
[32m+[m			[32mvel_x = (double) MAX_VEL;[m[41m[m
[32m+[m		[32m}else if (vel_x < -MAX_VEL){[m[41m[m
[32m+[m			[32mvel_x = (double) -MAX_VEL;}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mif (vel_y > MAX_VEL){[m[41m[m
[32m+[m			[32mvel_y = (float) MAX_VEL;[m[41m[m
[32m+[m		[32m}else if (vel_y < -MAX_VEL){[m[41m[m
[32m+[m			[32mvel_y = (float) -MAX_VEL;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mdouble k = (double)MAX_RANGE/MAX_VEL;[m[41m[m
[32m+[m		[32mif (Math.abs(vel_x) > 1) {[m[41m[m
[32m+[m			[32mvel_robot_x = (int) Math.round(k * vel_x * (1.0 + vel_y / 60.0));[m[41m[m
[32m+[m			[32mvel_robot_y = (int) Math.round(k * vel_x * (1.0 - vel_y / 60.0));[m[41m[m
[32m+[m		[32m} else {[m[41m[m
[32m+[m			[32mvel_robot_x = (int) Math.round(k * vel_y - Math.signum(vel_y) * k * Math.abs(vel_x));[m[41m[m
[32m+[m			[32mvel_robot_y = -vel_robot_x;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mif(vel_robot_x > MAX_RANGE)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_robot_x = MAX_RANGE;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32melse if (vel_robot_x < -MAX_RANGE)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_robot_x = -MAX_RANGE;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mif(vel_robot_y > MAX_RANGE)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_robot_y = MAX_RANGE;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32melse if (vel_robot_y < -MAX_RANGE)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_robot_y = -MAX_RANGE;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void get_speed()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//A partir de la aceleracion obtiene la velocidad a mover[m[41m[m
[32m+[m		[32mvel_x = accel_x;[m[41m[m
[32m+[m		[32mvel_y = accel_y;[m[41m[m
[32m+[m		[32mvel_z = accel_z;[m[41m[m
[32m+[m[41m		[m
[32m+[m				[32m//vel_robot=MAX_RANGE*vel/MAX_VEL[m[41m[m
[32m+[m[32m//		vel_robot_x = this.trans_vel_a_robot(vel_x);[m[41m[m
[32m+[m[32m//		vel_robot_y = this.trans_vel_a_robot(vel_y);[m[41m[m
[32m+[m[32m//		vel_robot_z = this.trans_vel_a_robot(vel_z);[m[41m[m
[32m+[m		[32mspeeds_to_robot();[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic int bar_percentage(int bar_range, double max_vel, double vel)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Ve cual es el porcentaje de barra que debe tener un valor dado[m[41m[m
[32m+[m		[32mint percentage = (int)Math.ceil((50.0*vel)/max_vel) + 50;[m[41m[m
[32m+[m		[32mreturn percentage;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic int bar_percentage(int bar_range, int center, double max_vel, double vel)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Ve el porcentaje que se tiene que mover, esta vez con un centro explicito[m[41m[m
[32m+[m		[32mint percentage = (int)Math.ceil(((bar_range-center)*vel)/max_vel) + center;[m[41m[m
[32m+[m		[32mreturn percentage;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void reset_speeds()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32mvel_x = 0;[m[41m[m
[32m+[m		[32mvel_y = 0;[m[41m[m
[32m+[m		[32mvel_z = 0;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void mostrar_velocidades_debug()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Muestra las velocidades actuales por debug[m[41m[m
[32m+[m		[32mSeekBar x_view = (SeekBar) getView().findViewById(R.id.seekBarX);[m[41m[m
[32m+[m		[32mSeekBar y_view = (SeekBar) getView().findViewById(R.id.seekBarY);[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m    x_view.setProgress(bar_percentage(100,MAX_VEL,this.vel_x));[m[41m[m
[32m+[m	[32m    y_view.setProgress(bar_percentage(100,MAX_VEL,this.vel_y));[m[41m[m
[32m+[m[41m	    [m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void send_speeds()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32m//Envia los datos obtenidos al robot.[m[41m[m
[32m+[m		[32mif(thisActivity.isConnected())[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m[41m			[m
[32m+[m[41m				[m
[32m+[m			[32m//Asumiendo que motor b y c sean los motores de las ruedas.[m[41m[m
[32m+[m			[32mint motorActionb = BTCommunicator.MOTOR_B;[m[41m[m
[32m+[m		[32m    int motorActionc = BTCommunicator.MOTOR_C;[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mthisActivity.sendBTCmessage(BTCommunicator.NO_DELAY, motorActionb,vel_robot_x , 0);[m[41m[m
[32m+[m			[32mthisActivity.sendBTCmessage(1000, motorActionb, 0, 0);[m[41m[m
[32m+[m			[32mthisActivity.sendBTCmessage(BTCommunicator.NO_DELAY, motorActionc, vel_robot_y, 0);[m[41m[m
[32m+[m			[32mthisActivity.sendBTCmessage(1000, motorActionc, 0, 0);[m[41m[m
[32m+[m[41m			[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32melse[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mreturn;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m@Override[m[41m[m

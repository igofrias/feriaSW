[32m+[m	[32m//Inicializacin lista de Achievements en la BD[m[41m[m
[32m+[m	[32mpublic void AchievementsList(Database_Helper helper)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32mhelper.open_read();[m[41m[m
[32m+[m		[32mCursor aux = helper.getAllAchievements();[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mif(aux.moveToFirst()){[m[41m [m
[32m+[m			[32m//Ya existe la tabla de Achievements registrada[m[41m[m
[32m+[m			[32mhelper.close();[m[41m[m
[32m+[m			[32mreturn;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mhelper.close();[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mhelper.open_write();[m[41m[m
[32m+[m		[32mhelper.createAchievement(ach1, ach1_desc, done);[m[41m[m
[32m+[m		[32mhelper.createAchievement(ach2, ach2_desc, done);[m[41m[m
[32m+[m		[32mhelper.createAchievement(ach3, ach3_desc, done);[m[41m[m
[32m+[m		[32mhelper.createAchievement(ach4, ach4_desc, done);[m[41m[m
[32m+[m		[32mhelper.close();[m[41m    	[m
[32m+[m	[32m}[m[41m[m

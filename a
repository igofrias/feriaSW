[32m+[m[32m    //retorna true si la bd esta vacia[m[41m[m
[32m+[m[32m    private boolean checkBD(){[m[41m[m
[32m+[m[41m    [m	[32mDatabase_Helper db = new Database_Helper(thisActivity);[m[41m[m
[32m+[m		[32mList<Pet> pets = db.getPets(); //lista de mascotas[m[41m[m
[32m+[m		[32mdb.close();[m[41m[m
[32m+[m		[32mreturn pets.isEmpty();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m	[32mpublic float getBatteryLevel() {[m[41m[m
[32m+[m	[32m    Intent batteryIntent = registerReceiver(null, new IntentFilter(Intent.ACTION_BATTERY_CHANGED));[m[41m[m
[32m+[m	[32m    int level = batteryIntent.getIntExtra(BatteryManager.EXTRA_LEVEL, -1);[m[41m[m
[32m+[m	[32m    int scale = batteryIntent.getIntExtra(BatteryManager.EXTRA_SCALE, -1);[m[41m[m
[32m+[m[41m[m
[32m+[m	[32m    // Error checking that probably isn't needed but I added just in case.[m[41m[m
[32m+[m	[32m    if(level == -1 || scale == -1) {[m[41m[m
[32m+[m	[32m        return 50.0f;[m[41m[m
[32m+[m	[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m	[32m    return ((float)level / (float)scale) * 100.0f;[m[41m [m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /////Detecta si el Bluetooth esta activado////////////[m[41m[m
[32m+[m[32m    public static boolean isBtOnByUs() {[m[41m[m
[32m+[m[32m        return btOnByUs;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m	[m
[32m+[m[32m    ///detecta si se activo el bluetooth////////[m[41m[m
[32m+[m[32m    public static void setBtOnByUs(boolean btOnByUs) {[m[41m[m
[32m+[m[32m        MainActivity.btOnByUs = btOnByUs;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    ///Crea un nuevo objeto para realizar la conexion///////////[m[41m[m
[32m+[m[32m    private void createBTCommunicator() {[m[41m[m
[32m+[m[32m        // interestingly BT adapter needs to be obtained by the UI thread - so we pass it in in the constructor[m[41m[m
[32m+[m[32m        myBTCommunicator = new BTCommunicator(this, myHandler, BluetoothAdapter.getDefaultAdapter(), getResources());[m[41m[m
[32m+[m[32m        btcHandler = myBTCommunicator.getHandler();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    //crea y arranca un thread para la conexion bluetooth/////////////[m[41m[m
[32m+[m[32m    //recibe la mac del robot/////////////[m[41m[m
[32m+[m[32m    public void startBTCommunicator(String mac_address) {[m[41m[m
[32m+[m[32m        mac_nxt= mac_address;[m[41m[m
[32m+[m[32m        connectingProgressDialog = ProgressDialog.show(this, "", getResources().getString(R.string.connecting_please_wait), true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (myBTCommunicator != null) {[m[41m[m
[32m+[m[32m            try {[m[41m[m
[32m+[m[32m                myBTCommunicator.destroyNXTconnection();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (IOException e) { }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        createBTCommunicator();[m[41m[m
[32m+[m[32m        myBTCommunicator.setMACAddress(mac_address);[m[41m[m
[32m+[m[32m        myBTCommunicator.start();[m[41m[m
[32m+[m[32m        connected=true;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    ////Termina la conexion bluetooth (destruye el thread)//////////[m[41m[m
[32m+[m[32m    public void destroyBTCommunicator() {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (myBTCommunicator != null) {[m[41m[m
[32m+[m[32m            sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.DISCONNECT, 0, 0);[m[41m[m
[32m+[m[32m            myBTCommunicator = null;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        connected = false;[m[41m[m
[32m+[m[32m        //updateButtonsAndMenu();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m   ///muestra el Toast///[m[41m[m
[32m+[m[32m    ////recive el mensaje y la duracion del Toast///////////[m[41m[m
[32m+[m[32m    private void showToast(String textToShow, int length) {[m[41m[m
[32m+[m[32m        reusableToast.setText(textToShow);[m[41m[m
[32m+[m[32m        reusableToast.setDuration(length);[m[41m[m
[32m+[m[32m        reusableToast.show();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    ///muestra el Toast///[m[41m[m
[32m+[m[32m    ////recive el mensaje  (int - ID) y la duracion del Toast///////////[m[41m[m
[32m+[m[32m    private void showToast(int resID, int length) {[m[41m[m
[32m+[m[32m        reusableToast.setText(resID);[m[41m[m
[32m+[m[32m        reusableToast.setDuration(length);[m[41m[m
[32m+[m[32m        reusableToast.show();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public String return_mac(){[m[41m[m
[32m+[m[41m    [m		[32mreturn mac_nxt;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public void selectTypeCnt(){[m[41m[m
[32m+[m[41m    [m	[32mif(connectionType!=null){//si contiene una mac conecta[m[41m[m
[32m+[m[41m    [m		[32mconnect(connectionType);[m[41m[m
[32m+[m[41m    [m	[32m}else{//si no parea[m[41m[m
[32m+[m[41m    [m		[32mpairing();[m[41m[m
[32m+[m[41m    [m	[32m}[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m   [m
[32m+[m[32m    public void connect(String mac){[m[41m[m
[32m+[m[41m    [m	[32mif (BluetoothAdapter.getDefaultAdapter()==null) {[m[41m[m
[32m+[m[32m            showToast(R.string.bt_initialization_failure, Toast.LENGTH_LONG);[m[41m[m
[32m+[m[32m            destroyBTCommunicator();[m[41m[m
[32m+[m[32m            finish();[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m    [m	[32mconnectionType = mac;[m[41m[m
[32m+[m[41m    [m	[32mif (!BluetoothAdapter.getDefaultAdapter().isEnabled()) {[m[41m[m
[32m+[m[32m            Intent enableIntent = new Intent(BluetoothAdapter.ACTION_REQUEST_ENABLE);[m[41m[m
[32m+[m[32m            startActivityForResult(enableIntent, REQUEST_ENABLE_BT);[m[41m[m
[32m+[m[32m        } else {//si esta activado busca dispositivos para conectarse[m[41m[m
[32m+[m[41m        [m	[32mstartBTCommunicator(mac);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m    	[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    //conexion intent a dispositivo[m[41m[m
[32m+[m	[32mpublic void pairing(){[m[41m[m
[32m+[m[41m    [m	[32m//si no esta conectado verifica presencia de bluetooth[m[41m [m
[32m+[m		[32mif(connected==false){[m[41m[m
[32m+[m			[32m///inicia el adaptador[m[41m[m
[32m+[m			[32mif (BluetoothAdapter.getDefaultAdapter()==null) {[m[41m[m
[32m+[m		[32m            showToast(R.string.bt_initialization_failure, Toast.LENGTH_LONG);[m[41m[m
[32m+[m		[32m            destroyBTCommunicator();[m[41m[m
[32m+[m		[32m            finish();[m[41m[m
[32m+[m		[32m            return;[m[41m[m
[32m+[m		[32m        }[m[41m            [m
[32m+[m				[32m//si existe blublu y no esta activado lo activa[m[41m[m
[32m+[m				[32mconnectionType=null;[m[41m[m
[32m+[m		[32m        if (!BluetoothAdapter.getDefaultAdapter().isEnabled()) {[m[41m[m
[32m+[m		[32m            Intent enableIntent = new Intent(BluetoothAdapter.ACTION_REQUEST_ENABLE);[m[41m[m
[32m+[m		[32m            startActivityForResult(enableIntent, REQUEST_ENABLE_BT);[m[41m		     [m
[32m+[m		[32m        } else {//si esta activado busca dispositivos para conectarse[m[41m[m
[32m+[m		[32m           selectNXT();[m[41m[m
[32m+[m		[32m        }[m[41m[m
[32m+[m[41m		        [m
[32m+[m		[32m}else if(connected==true){[m[41m[m
[32m+[m			[32m//si esta conectado desconecta[m[41m[m
[32m+[m			[32mdestroyBTCommunicator();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m[41m    [m
[32m+[m	[32m@Override[m[41m[m
[32m+[m[32m    protected void onDestroy() {[m[41m[m
[32m+[m[32m        super.onDestroy();[m[41m[m
[32m+[m[32m        destroyBTCommunicator();[m[41m[m
[32m+[m[32m        if (btOnByUs){[m[41m[m
[32m+[m[32m            showToast(R.string.bt_off_message, Toast.LENGTH_SHORT);[m[41m[m
[32m+[m[32m            BluetoothAdapter.getDefaultAdapter().disable();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m         [m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    public void onPause() {[m[41m[m
[32m+[m[32m        destroyBTCommunicator();[m[41m[m
[32m+[m[32m        super.onStop();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    public void onSaveInstanceState(Bundle icicle) {[m[41m[m
[32m+[m[32m        super.onSaveInstanceState(icicle);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m	 [m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Starts a program on the NXT robot.[m[41m[m
[32m+[m[32m     * @param name The program name to start. Has to end with .rxe on the LEGO firmware and with .nxj on the[m[41m [m
[32m+[m[32m     *             leJOS NXJ firmware.[m[41m[m
[32m+[m[32m     */[m[41m   [m
[32m+[m[32m    public void startProgram(String name) {[m[41m[m
[32m+[m[32m        // for .rxe programs: get program name, eventually stop this and start the new one delayed[m[41m[m
[32m+[m[32m        // is handled in startRXEprogram()[m[41m[m
[32m+[m[32m        if (name.endsWith(".rxe")) {[m[41m[m
[32m+[m[41m        [m	[32mLog.d("pepe", "rxe");[m[41m[m
[32m+[m[32m            programToStart = name;[m[41m        [m
[32m+[m[32m            sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.GET_PROGRAM_NAME, 0, 0);[m[41m[m
[32m+[m[32m            Log.d("pepe", "startprogram fin D:");[m[41m[m
[32m+[m[32m            //return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m              [m
[32m+[m[32m       /// for .nxj programs: stop bluetooth communication after starting the program[m[41m[m
[32m+[m[32m        /*if (name.endsWith(".nxj")) {[m[41m[m
[32m+[m[32m            sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.START_PROGRAM, name);[m[41m[m
[32m+[m[32m            destroyBTCommunicator();[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        } */[m[41m      [m
[32m+[m[32m        Log.d("pepe", "start!!!!!!!!!!!!!!!");[m[41m[m
[32m+[m[32m        // for all other programs: just start the program[m[41m[m
[32m+[m[32m        sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.START_PROGRAM, name);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Depending on the status (whether the program runs already) we stop it, wait and restart it again.[m[41m[m
[32m+[m[32m     * @param status The current status, 0x00 means that the program is already running.[m[41m[m
[32m+[m[32m     */[m[41m   [m
[32m+[m[32m    public void startRXEprogram(byte status) {[m[41m[m
[32m+[m[32m        if (status == 0x00) {[m[41m[m
[32m+[m[41m        [m	[32m Log.d("pepe", "status 0x00");[m[41m[m
[32m+[m[32m            sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.STOP_PROGRAM, 0, 0);[m[41m[m
[32m+[m[32m            sendBTCmessage(1000, BTCommunicator.START_PROGRAM, programToStart);[m[41m[m
[32m+[m[32m        }[m[41m    [m
[32m+[m[32m        else {[m[41m[m
[32m+[m[41m        [m	[32m Log.d("pepe", "no status 0x00");[m[41m[m
[32m+[m[32m            sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.START_PROGRAM, programToStart);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m   [m
[32m+[m[41m    [m
[32m+[m[32m    ///envia al bthandler los mensajes via blublu   (enteros)[m[41m[m
[32m+[m[32m    void sendBTCmessage(int delay, int message, int value1, int value2) {[m[41m[m
[32m+[m[32m        Bundle myBundle = new Bundle();[m[41m[m
[32m+[m[32m        myBundle.putInt("message", message);[m[41m[m
[32m+[m[32m        myBundle.putInt("value1", value1);[m[41m[m
[32m+[m[32m        myBundle.putInt("value2", value2);[m[41m[m
[32m+[m[32m        Message myMessage = myHandler.obtainMessage();[m[41m[m
[32m+[m[32m        myMessage.setData(myBundle);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (delay == 0)[m[41m[m
[32m+[m[32m            btcHandler.sendMessage(myMessage);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        else[m[41m[m
[32m+[m[32m            btcHandler.sendMessageDelayed(myMessage, delay);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m  ///envia al bthandler los mensajes via blublu   (string)[m[41m  [m
[32m+[m[32m    void sendBTCmessage(int delay, int message, String name) {[m[41m[m
[32m+[m[32m        Bundle myBundle = new Bundle();[m[41m[m
[32m+[m[32m        myBundle.putInt("message", message);[m[41m[m
[32m+[m[32m        myBundle.putString("name", name);[m[41m[m
[32m+[m[32m        Message myMessage = myHandler.obtainMessage();[m[41m[m
[32m+[m[32m        myMessage.setData(myBundle);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (delay == 0)[m[41m[m
[32m+[m[32m            btcHandler.sendMessage(myMessage);[m[41m[m
[32m+[m[32m        else[m[41m[m
[32m+[m[32m            btcHandler.sendMessageDelayed(myMessage, delay);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    //llama a la actividad que  busca dispositivos[m[41m[m
[32m+[m[32m    void selectNXT() {[m[41m[m
[32m+[m[32m        Intent serverIntent = new Intent(this, DeviceListActivity.class);[m[41m[m
[32m+[m[32m        startActivityForResult(serverIntent, REQUEST_CONNECT_DEVICE);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    //ejecuta la accion del boton 1, toca mozart y mueve el motor A[m[41m[m

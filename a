[32m+[m	[32mprivate class DrawJoystick extends SurfaceView implements SurfaceHolder.Callback, Runnable[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Clase que maneja el dibujo del joystick. Tiene un thread que llama a que[m[41m[m
[32m+[m		[32m//se dibuje en el canvas[m[41m[m
[32m+[m		[32mDouble vel_x;[m[41m[m
[32m+[m		[32mDouble vel_y;[m[41m[m
[32m+[m		[32mPaint color;[m[41m[m
[32m+[m		[32mPaint color_center;[m[41m[m
[32m+[m		[32mSurfaceHolder hold;[m[41m[m
[32m+[m		[32mCanvas can;[m[41m[m
[32m+[m		[32mThread drawthread;[m[41m[m
[32m+[m		[32mBoolean running;[m[41m[m
[32m+[m		[32mpublic DrawJoystick(Context context) {[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32msuper(context);[m[41m[m
[32m+[m			[32mcolor = new Paint();[m[41m[m
[32m+[m			[32mgetHolder().addCallback(this);[m[41m[m
[32m+[m			[32mcolor.setColor(Color.GREEN);[m[41m[m
[32m+[m			[32mcolor_center = new Paint();[m[41m[m
[32m+[m			[32mcolor_center.setColor(Color.GRAY);[m[41m[m
[32m+[m			[32mvel_x = 0.0;[m[41m[m
[32m+[m			[32mvel_y = 0.0;[m[41m[m
[32m+[m			[32mrunning = false;[m[41m[m
[32m+[m			[32m// TODO Auto-generated constructor stub[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m@Override[m[41m[m
[32m+[m		[32mpublic void surfaceChanged(SurfaceHolder holder, int format, int width,[m[41m[m
[32m+[m				[32mint height) {[m[41m[m
[32m+[m			[32m// TODO Auto-generated method stub[m[41m[m
[32m+[m[41m			[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m@Override[m[41m[m
[32m+[m		[32mpublic void surfaceCreated(SurfaceHolder holder) {[m[41m[m
[32m+[m			[32m// TODO Auto-generated method stub[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mhold = canvas.getHolder();[m[41m[m
[32m+[m			[32mrunning = true;[m[41m[m
[32m+[m			[32mdrawthread = new Thread(this);[m[41m[m
[32m+[m			[32mdrawthread.start();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m@Override[m[41m[m
[32m+[m		[32mpublic void surfaceDestroyed(SurfaceHolder holder) {[m[41m[m
[32m+[m			[32m// TODO Auto-generated method stub[m[41m[m
[32m+[m			[32mrunning = false;[m[41m[m
[32m+[m			[32mBoolean retry = true;[m[41m[m
[32m+[m			[32mwhile(retry)[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m				[32mtry {[m[41m[m
[32m+[m[41m					[m
[32m+[m					[32mdrawthread.join();[m[41m[m
[32m+[m					[32mretry = false;[m[41m[m
[32m+[m[41m					[m
[32m+[m				[32m} catch (InterruptedException e) {[m[41m[m
[32m+[m					[32m// TODO Auto-generated catch block[m[41m[m
[32m+[m					[32me.printStackTrace();[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m		[32mpublic void Draw(Canvas canvas)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mfloat center_x = canvas.getWidth()/2;[m[41m[m
[32m+[m			[32mfloat center_y = canvas.getHeight()/2;[m[41m[m
[32m+[m			[32mint size = 30;[m[41m[m
[32m+[m			[32mint x = bar_percentage(canvas.getWidth() - size, (int)center_x, -MAX_VEL, vel_x);[m[41m[m
[32m+[m			[32mint y = bar_percentage(canvas.getHeight() - size, (int)center_y,MAX_VEL, vel_y);[m[41m[m
[32m+[m			[32mcanvas.drawARGB(255, 255, 255, 255);[m[41m[m
[32m+[m			[32mcanvas.drawCircle(canvas.getWidth()/2,canvas.getHeight()/2,100, color_center);[m[41m[m
[32m+[m			[32mcanvas.drawCircle(x, y , size, color);[m[41m[m
[32m+[m[41m			[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mpublic void update_coordinates(Double x, Double y)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mvel_x = x;[m[41m[m
[32m+[m			[32mvel_y = y;[m[41m[m
[32m+[m[41m			[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m@Override[m[41m[m
[32m+[m		[32mpublic void run() {[m[41m[m
[32m+[m			[32m// TODO Auto-generated method stub[m[41m[m
[32m+[m[41m			[m
[32m+[m			[32mwhile(running){[m[41m[m
[32m+[m				[32mif(hold.getSurface().isValid())[m[41m[m
[32m+[m				[32m{[m[41m[m
[32m+[m					[32mcan = hold.lockCanvas();[m[41m[m
[32m+[m[41m				[m
[32m+[m[41m					[m
[32m+[m						[32mcanvas.Draw(can);[m[41m[m
[32m+[m[41m					[m
[32m+[m					[32mhold.unlockCanvasAndPost(can);[m[41m[m
[32m+[m				[32m}[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[32m}[m[41m[m

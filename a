[32m+[m	[32m//agregar entrada nueva[m[41m[m
[32m+[m	[32mpublic void createEntry(String nombre, String datetime, String raza, String color, String mac_adress, int death) {[m[41m[m
[32m+[m		[32mContentValues cv = new ContentValues();[m[41m[m
[32m+[m		[32mcv.put(Key_name, nombre);[m[41m[m
[32m+[m		[32mcv.put(Key_date, datetime);[m[41m[m
[32m+[m		[32mcv.put(Key_raza, raza);[m[41m[m
[32m+[m		[32mcv.put(Key_color, color);[m[41m[m
[32m+[m		[32mcv.put(Key_mac, mac_adress);[m[41m[m
[32m+[m		[32mcv.put(Key_death, death);[m[41m[m
[32m+[m		[32mDatabase.insert(DB_table, null, cv);[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m//obtener datos por el nombre[m[41m[m
[32m+[m	[32mpublic Cursor getEntry(String nombre) {[m[41m[m
[32m+[m		[32mString select = "Select * From "+DB_table+" Where "+Key_name+" = '"+nombre+"'";[m[41m[m
[32m+[m[41m    [m	[32mCursor c = Database.rawQuery(select, null);[m[41m[m
[32m+[m		[32mreturn c;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m//obtener todo[m[41m[m
[32m+[m	[32mpublic Cursor getAll() {[m[41m[m
[32m+[m		[32mString select = "Select *From "+DB_table;[m[41m[m
[32m+[m		[32mselect = select + " ORDER BY "+Key_id+" DESC";[m[41m[m
[32m+[m[41m    [m	[32mCursor c = Database.rawQuery(select, null);[m[41m[m
[32m+[m		[32mreturn c;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m[41m[m
[32m+[m	[32mpublic List<Pet> getPets() {[m[41m[m
[32m+[m		[32mList<Pet> listamascotas = new ArrayList<Pet>();[m[41m[m
[32m+[m		[32m// Select All Query[m[41m[m
[32m+[m		[32mString selectQuery = "SELECT * FROM " + DB_table + " WHERE " + Key_death + " = 0  ORDER BY " + Key_id + " DESC";[m[41m[m
[32m+[m[41m[m
[32m+[m		[32mhelper = new DBhelper(context);[m[41m[m
[32m+[m		[32mDatabase = helper.getWritableDatabase();[m[41m[m
[32m+[m		[32mCursor cursor = Database.rawQuery(selectQuery, null);[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m// looping through all rows and adding to list[m[41m[m
[32m+[m		[32mif (cursor.moveToFirst()) {[m[41m[m
[32m+[m			[32mdo {[m[41m[m
[32m+[m				[32mPet pet = new Pet();[m[41m[m
[32m+[m				[32mpet.set_id(Integer.parseInt(cursor.getString(0)));[m[41m[m
[32m+[m				[32mpet.set_name(cursor.getString(1));[m[41m[m
[32m+[m				[32mpet.set_raza(cursor.getString(2));[m[41m[m
[32m+[m				[32mpet.set_color(cursor.getString(3));[m[41m[m
[32m+[m				[32mpet.set_birthdate(cursor.getString(4));[m[41m[m
[32m+[m				[32mpet.set_mac(cursor.getString(5));[m[41m[m
[32m+[m				[32mpet.set_death(Integer.parseInt(cursor.getString(6)));[m[41m[m
[32m+[m				[32m// agregar mascota a lista[m[41m[m
[32m+[m				[32mlistamascotas.add(pet);[m[41m[m
[32m+[m			[32m} while (cursor.moveToNext());[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mDatabase.close();[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreturn listamascotas;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m//borrar por nombre[m[41m[m
[32m+[m	[32mpublic void delete(String query) {[m[41m[m
[32m+[m		[32mDatabase.delete(DB_table, Key_name + " = '"+query+"'", null);[m[41m[m
[32m+[m	[32m}[m[41m[m

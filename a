[32m+[m	[32m/**********setea el tiempo de vida de la mascota[m[41m[m
[32m+[m	[32m * calcula el tiempo de vida ylos transforma den dias, minutos u horas segun corresponda D:[m[41m[m
[32m+[m	[32m * recibe un estring con la fecha de nacimiento[m[41m[m
[32m+[m	[32m * @param birthdate[m[41m[m
[32m+[m	[32m */[m[41m[m
[32m+[m	[32mvoid setlifetime(String birthdate){[m[41m[m
[32m+[m		[32mif (((int) (lifetimecalc(birthdate) / (60*60*24))==0))[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mif (((int) (lifetimecalc(birthdate) / (60*60)))==0){[m[41m[m
[32m+[m				[32mif(((int)lifetimecalc(birthdate) / (60))==1){[m[41m[m
[32m+[m					[32mlifetime.setText(((int)lifetimecalc(birthdate) / (60)) + " Minuto");[m[41m[m
[32m+[m				[32m}else{[m[41m[m
[32m+[m					[32mlifetime.setText(((int)lifetimecalc(birthdate) / (60)) + " Minutos");[m[41m[m
[32m+[m				[32m}[m[41m		[m
[32m+[m			[32m}else{[m[41m[m
[32m+[m				[32mif(((int)lifetimecalc(birthdate) / (60*60))==1){[m[41m[m
[32m+[m					[32mlifetime.setText(((int)lifetimecalc(birthdate) / (60*60)) + " Hora");[m[41m[m
[32m+[m				[32m}else{[m[41m[m
[32m+[m					[32mlifetime.setText(((int)lifetimecalc(birthdate) / (60*60)) + " Horas");[m[41m[m
[32m+[m				[32m}[m[41m	[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}else{[m[41m[m
[32m+[m			[32mif(((int)lifetimecalc(birthdate) / (60*60*24))==1){[m[41m[m
[32m+[m				[32mlifetime.setText(((int)lifetimecalc(birthdate) / (60*60*24)) + " Da");[m[41m[m
[32m+[m			[32m}else{[m[41m[m
[32m+[m				[32mlifetime.setText(((int)lifetimecalc(birthdate) / (60*60*24)) + " Das");[m[41m[m
[32m+[m			[32m}[m[41m	[m
[32m+[m		[32m}[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m/*********ReadBD[m[41m[m
[32m+[m	[32m * lee los dos de la mascota de la base de datos[m[41m[m
[32m+[m	[32m */[m[41m[m
[32m+[m	[32mvoid readBD(){[m[41m [m

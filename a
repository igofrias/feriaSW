[32m+[m[32m//Maneja el control remoto usando acelerometro[m[41m[m
[32m+[m[32m@TargetApi(Build.VERSION_CODES.HONEYCOMB)[m[41m[m
[32m+[m[32mpublic class RemoteControl extends SherlockFragment implements SensorEventListener{[m[41m[m
[32m+[m	[32mprivate int MAX_RANGE;[m[41m[m
[32m+[m	[32mprivate double MAX_VEL;[m[41m[m
[32m+[m	[32mprivate Sensor accelerometer;[m[41m[m
[32m+[m	[32mSensorManager manager;[m[41m[m
[32m+[m	[32mdouble raw_accel_x = 0;[m[41m[m
[32m+[m	[32mdouble raw_accel_y = 0;[m[41m[m
[32m+[m	[32mdouble raw_accel_z = 0;[m[41m[m
[32m+[m	[32mdouble accel_x = 0;[m[41m[m
[32m+[m	[32mdouble accel_y = 0;[m[41m[m
[32m+[m	[32mdouble accel_z = 0;[m[41m[m
[32m+[m	[32mdouble gravity[];[m[41m[m
[32m+[m[32m    boolean running=true;[m[41m[m
[32m+[m	[32mdouble vel_x = 0;[m[41m[m
[32m+[m	[32mdouble vel_y = 0;[m[41m[m
[32m+[m	[32mdouble vel_z = 0;[m[41m[m
[32m+[m	[32mint vel_robot_x = 0;[m[41m[m
[32m+[m	[32mint vel_robot_y = 0;[m[41m[m
[32m+[m	[32mint vel_robot_z = 0;[m[41m[m
[32m+[m	[32mdouble sens;[m[41m[m
[32m+[m	[32mThread thMesseger;[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32mDrawJoystick canvas;[m[41m[m
[32m+[m	[32mActivity parent_activity;[m[41m[m
[32m+[m	[32mMainActivity thisActivity;[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic void onCreate(Bundle savedInstanceState) {[m[41m[m
[32m+[m		[32msuper.onCreate(savedInstanceState);[m[41m[m

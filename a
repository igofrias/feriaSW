[32m+[m	[32mprivate void read_db() {[m[41m[m
[32m+[m		[32mdb = new Database_Helper(thisActivity);[m[41m[m
[32m+[m	[32m    db.open_read();[m[41m[m
[32m+[m	[32m    Cursor c = db.getAll();		//llenando el cursor con datos[m[41m [m
[32m+[m	[32m    //adapter = new Cursor_Adapter(thisActivity, R.layout.row, c, from, to);		//creando el adapter[m[41m[m
[32m+[m	[32m   // lista.setAdapter(adapter);		//entregandole el adapter a la lista[m[41m[m
[32m+[m	[32m    db.close();[m[41m[m
[32m+[m	[32m}[m[41m [m
[32m+[m	[32m/********* lifetimecalc[m[41m[m
[32m+[m	[32m *  recibe la cfecha de nacimiento de la mascota[m[41m[m
[32m+[m	[32m *  y retorna el tiempo de vida en segundos[m[41m[m
[32m+[m	[32m * @param birthdate[m[41m[m
[32m+[m	[32m * @return diff[m[41m[m
[32m+[m	[32m */[m[41m[m
[32m+[m	[32mprivate long lifetimecalc(String birthdate){[m[41m[m
[32m+[m		[32mSimpleDateFormat format = new SimpleDateFormat("yy-MM-dd HH:mm:ss");[m[41m[m
[32m+[m		[32mDate date1= new Date();[m[41m[m
[32m+[m		[32mDate date2= new Date();[m[41m[m
[32m+[m		[32mSimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd HH:mm:ss");[m[41m[m
[32m+[m		[32mString currentDateandTime = sdf.format(new Date());//si hay nombre crea el bicho en la base de datos[m[41m[m
[32m+[m		[32mtry {[m[41m[m
[32m+[m			[32mdate1 = format.parse(birthdate);[m[41m[m
[32m+[m			[32mdate2= format.parse(currentDateandTime);[m[41m[m
[32m+[m		[32m} catch (ParseException e) {[m[41m[m
[32m+[m			[32m// TODO Auto-generated catch block[m[41m[m
[32m+[m			[32me.printStackTrace();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mlong diff = date2.getTime() - date1.getTime();[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mreturn diff/1000;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m/********Task[m[41m[m
[32m+[m	[32m * corre tareas en paralelo y se encarga de actualizar los datos de la[m[41m [m
[32m+[m	[32m * mascota mientras corre la app[m[41m[m
[32m+[m	[32m * @author Neko-sama[m[41m[m
[32m+[m	[32m *[m[41m[m
[32m+[m	[32m */[m[41m[m
[32m+[m[32m    //task para realizar tareas paralelas[m[41m[m
[32m+[m[32m    private class Task extends AsyncTask<Void, Integer, Void>{[m[41m[m
[32m+[m[41m		[m
[32m+[m[41m    	[m
[32m+[m[41m[m
[32m+[m		[32mpublic Task(StatesActivity tarea) {[m[41m[m
[32m+[m			[32m// TODO Auto-generated constructor stub[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m@Override[m[41m[m
[32m+[m	[32m    protected void onPreExecute() {[m[41m[m

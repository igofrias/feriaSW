[32m+[m			[32m}[m[41m[m
[32m+[m		[32m});[m[41m[m
[32m+[m		[32mthread.setRunning(true);[m[41m[m
[32m+[m		[32mthread.start();[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mif(checkBD()){[m[41m[m
[32m+[m			[32mlaunch_create();[m[41m[m
[32m+[m		[32m}else{[m[41m[m
[32m+[m			[32mlaunch_states();[m[41m[m
[32m+[m			[32mlaunch_mainpet();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    //llama al supa framento[m[41m[m
[32m+[m[32m    void launch_create() {//identificamos y cargamos el fragmento menu[m[41m[m
[32m+[m		[32mFragmentTransaction ft = getSupportFragmentManager().beginTransaction();[m[41m[m
[32m+[m		[32mSherlockFragment fragment = ((CreateActivity)getSupportFragmentManager().findFragmentByTag("create"));[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mif(fragment==null){[m[41m[m
[32m+[m			[32mfragment = new CreateActivity();[m[41m[m
[32m+[m			[32mft.add(R.id.linear, fragment,"create");[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32melse{[m[41m[m
[32m+[m			[32mif(fragment.isDetached()){[m[41m[m
[32m+[m				[32mft.attach(fragment);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mft.commit();[m[41m[m
[32m+[m	[32m}[m[41m[m

[32m+[m[32mimport android.view.View.OnClickListener;[m[41m[m
[32m+[m[32mimport android.widget.AdapterView;[m[41m[m
[32m+[m[32mimport android.widget.ArrayAdapter;[m[41m[m
[32m+[m[32mimport android.widget.Button;[m[41m[m
[32m+[m[32mimport android.widget.ListView;[m[41m[m
[32m+[m[32mimport android.widget.TextView;[m[41m[m
[32m+[m[32mimport android.widget.AdapterView.OnItemClickListener;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m/**[m[41m[m
[32m+[m[32m * This Activity appears as a dialog. It lists any paired devices and[m[41m[m
[32m+[m[32m * devices detected in the area after discovery. When a device is chosen[m[41m[m
[32m+[m[32m * by the user, the MAC address of the device is sent back to the parent[m[41m[m
[32m+[m[32m * Activity in the result Intent.[m[41m[m
[32m+[m[32m */[m[41m[m
[32m+[m[32mpublic class DeviceListActivity extends Activity {[m[41m[m
[32m+[m[32m    static final String PAIRING = "pairing";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // Return Intent extra[m[41m[m
[32m+[m[32m    public static String DEVICE_NAME_AND_ADDRESS = "device_infos";[m[41m[m
[32m+[m[32m    public static String EXTRA_DEVICE_ADDRESS = "device_address";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // Member fields[m[41m[m
[32m+[m[32m    private BluetoothAdapter mBtAdapter;[m[41m[m
[32m+[m[32m    private ArrayAdapter<String> mPairedDevicesArrayAdapter;[m[41m[m
[32m+[m[32m    private ArrayAdapter<String> mNewDevicesArrayAdapter;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m[41m[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Setup the window[m[41m[m
[32m+[m[32m        requestWindowFeature(Window.FEATURE_INDETERMINATE_PROGRESS);//gargando en ek actionbar[m[41m[m
[32m+[m[32m        setContentView(R.layout.device_list);[m[41m[m
[32m+[m[32m        // Set result CANCELLED in case the user backs out[m[41m[m
[32m+[m[32m        setResult(Activity.RESULT_CANCELED);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Initialize the button to perform device discovery[m[41m[m
[32m+[m[32m        Button scanButton = (Button) findViewById(R.id.button_scan);[m[41m[m
[32m+[m[32m        scanButton.setOnClickListener(new OnClickListener() {[m[41m[m
[32m+[m[32m            public void onClick(View v) {[m[41m[m
[32m+[m[32m                doDiscovery();[m[41m[m
[32m+[m[32m                v.setVisibility(View.GONE);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Initialize array adapters. One for already paired devices and[m[41m[m
[32m+[m[32m        // one for newly discovered devices[m[41m[m
[32m+[m[32m        mPairedDevicesArrayAdapter = new ArrayAdapter<String>(this, R.layout.device_name);[m[41m[m
[32m+[m[32m        mNewDevicesArrayAdapter = new ArrayAdapter<String>(this, R.layout.device_name);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Find and set up the ListView for paired devices[m[41m[m
[32m+[m[32m        ListView pairedListView = (ListView) findViewById(R.id.paired_devices);[m[41m[m
[32m+[m[32m        pairedListView.setAdapter(mPairedDevicesArrayAdapter);[m[41m[m
[32m+[m[32m        pairedListView.setOnItemClickListener(mDeviceClickListener);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Find and set up the ListView for newly discovered devices[m[41m[m
[32m+[m[32m        ListView newDevicesListView = (ListView) findViewById(R.id.new_devices);[m[41m[m
[32m+[m[32m        newDevicesListView.setAdapter(mNewDevicesArrayAdapter);[m[41m[m
[32m+[m[32m        newDevicesListView.setOnItemClickListener(mDeviceClickListener);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Register for broadcasts when a device is discovered[m[41m[m
[32m+[m[32m        IntentFilter filter = new IntentFilter(BluetoothDevice.ACTION_FOUND);[m[41m[m
[32m+[m[32m        this.registerReceiver(mReceiver, filter);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Register for broadcasts when discovery has finished[m[41m[m
[32m+[m[32m        filter = new IntentFilter(BluetoothAdapter.ACTION_DISCOVERY_FINISHED);[m[41m[m
[32m+[m[32m        this.registerReceiver(mReceiver, filter);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Get the local Bluetooth adapter[m[41m[m
[32m+[m[32m        mBtAdapter = BluetoothAdapter.getDefaultAdapter();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Get a set of currently paired devices[m[41m[m
[32m+[m[32m        Set<BluetoothDevice> pairedDevices = mBtAdapter.getBondedDevices();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If there are paired devices, add each one to the ArrayAdapter[m[41m[m
[32m+[m[32m        boolean legoDevicesFound = false;[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        if (pairedDevices.size() > 0) {[m[41m[m
[32m+[m[32m            findViewById(R.id.title_paired_devices).setVisibility(View.VISIBLE);[m[41m[m
[32m+[m[32m            for (BluetoothDevice device : pairedDevices) {[m[41m[m
[32m+[m[32m                // only add LEGO devices[m[41m[m
[32m+[m[32m                if (device.getAddress().startsWith(BTCommunicator.OUI_LEGO)) {[m[41m[m
[32m+[m[32m                    legoDevicesFound = true;[m[41m[m
[32m+[m[32m                    mPairedDevicesArrayAdapter.add(device.getName() + "-" + device.getAddress());[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        if (legoDevicesFound == false) {[m[41m[m
[32m+[m[32m            String noDevices = getResources().getText(R.string.none_paired).toString();[m[41m[m
[32m+[m[32m            mPairedDevicesArrayAdapter.add(noDevices);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    protected void onDestroy() {[m[41m[m
[32m+[m[32m        super.onDestroy();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Make sure we're not doing discovery anymore[m[41m[m
[32m+[m[32m        if (mBtAdapter != null) {[m[41m[m
[32m+[m[32m            mBtAdapter.cancelDiscovery();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Unregister broadcast listeners[m[41m[m
[32m+[m[32m        this.unregisterReceiver(mReceiver);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /**[m[41m[m
[32m+[m[32m     * Start device discover with the BluetoothAdapter[m[41m[m
[32m+[m[32m     */[m[41m[m
[32m+[m[32m    private void doDiscovery() {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Indicate scanning in the title[m[41m[m
[32m+[m[32m        setProgressBarIndeterminateVisibility(true);[m[41m[m
[32m+[m[32m        setTitle(R.string.scanning);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Turn on sub-title for new devices[m[41m[m
[32m+[m[32m        findViewById(R.id.title_new_devices).setVisibility(View.VISIBLE);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // If we're already discovering, stop it[m[41m[m
[32m+[m[32m        if (mBtAdapter.isDiscovering()) {[m[41m[m
[32m+[m[32m            mBtAdapter.cancelDiscovery();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // Request discover from BluetoothAdapter[m[41m[m
[32m+[m[32m        mBtAdapter.startDiscovery();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // The on-click listener for all devices in the ListViews[m[41m[m
[32m+[m[32m    private OnItemClickListener mDeviceClickListener = new OnItemClickListener() {[m[41m[m
[32m+[m[32m        public void onItemClick(AdapterView<?> av, View v, int arg2, long arg3) {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            String info = ((TextView) v).getText().toString();[m[41m[m
[32m+[m[32m            // did we choose a correct name and address?[m[41m[m
[32m+[m[32m            if (info.lastIndexOf('-') != info.length()-18)[m[41m [m
[32m+[m[32m                return;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Cancel discovery because it's costly and we're about to connect[m[41m[m
[32m+[m[32m            mBtAdapter.cancelDiscovery();[m[41m[m
[32m+[m[32m            // Get the device MAC address, this is the text after the last '-' character[m[41m[m
[32m+[m[32m            String address = info.substring(info.lastIndexOf('-')+1);[m[41m[m
[32m+[m[32m            // Create the result Intent and include the infos[m[41m[m
[32m+[m[32m            Intent intent = new Intent();[m[41m[m
[32m+[m[32m            Bundle data = new Bundle();[m[41m[m
[32m+[m[32m            data.putString(DEVICE_NAME_AND_ADDRESS, info);[m[41m[m
[32m+[m[32m            data.putString(EXTRA_DEVICE_ADDRESS, address);[m[41m[m
[32m+[m[32m            data.putBoolean(PAIRING,av.getId()==R.id.new_devices);[m[41m[m
[32m+[m[32m            intent.putExtras(data);[m[41m[m
[32m+[m[32m            // Set result and finish this Activity[m[41m[m
[32m+[m[32m            setResult(RESULT_OK, intent);[m[41m[m
[32m+[m[32m            finish();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    // The BroadcastReceiver that listens for discovered devices and[m[41m[m
[32m+[m[32m    // changes the title when discovery is finished[m[41m[m
[32m+[m[32m    private final BroadcastReceiver mReceiver = new BroadcastReceiver() {[m[41m[m
[32m+[m[32m        @Override[m[41m[m
[32m+[m[32m        public void onReceive(Context context, Intent intent) {[m[41m[m
[32m+[m[32m            String action = intent.getAction();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // When discovery finds a device[m[41m[m
[32m+[m[32m            if (BluetoothDevice.ACTION_FOUND.equals(action)) {[m[41m[m
[32m+[m[32m                // Get the BluetoothDevice object from the Intent[m[41m[m
[32m+[m[32m                BluetoothDevice device = intent.getParcelableExtra(BluetoothDevice.EXTRA_DEVICE);[m[41m[m
[32m+[m[32m                // If it's already paired, skip it, because it's been listed already[m[41m[m
[32m+[m[32m                if (device.getBondState() != BluetoothDevice.BOND_BONDED) {[m[41m[m
[32m+[m[32m                    mNewDevicesArrayAdapter.add(device.getName() + "-" + device.getAddress());[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            // When discovery is finished, change the Activity title[m[41m[m
[32m+[m[32m            } else if (BluetoothAdapter.ACTION_DISCOVERY_FINISHED.equals(action)) {[m[41m[m
[32m+[m[32m                setProgressBarIndeterminateVisibility(false);[m[41m[m
[32m+[m[32m                setTitle(R.string.select_device);[m[41m[m
[32m+[m[32m                if (mNewDevicesArrayAdapter.getCount() == 0) {[m[41m[m
[32m+[m[32m                    String noDevices = getResources().getText(R.string.none_found).toString();[m[41m[m
[32m+[m[32m                    mNewDevicesArrayAdapter.add(noDevices);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    };[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m[m
[32m+[m[32m}[m[41m[m

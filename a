[32m+[m		[32mreusableToast = Toast.makeText(thisActivity, "", Toast.LENGTH_SHORT);[m[41m[m
[32m+[m		[32mthread = new ThreadClass(new Handler() {[m[41m[m
[32m+[m			[32m@Override[m[41m[m
[32m+[m			[32mpublic void handleMessage(Message m) {[m[41m[m

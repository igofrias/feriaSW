[32m+[m[32m    ///ejecuta funciones para caminar[m[41m[m
[32m+[m[32m    private void caminar(int direccion) {[m[41m[m
[32m+[m		[32mmotorActionb = BTCommunicator.MOTOR_B;[m[41m[m
[32m+[m		[32mmotorActionc = BTCommunicator.MOTOR_C;[m[41m[m
[32m+[m		[32mdirectionAction = 1;[m[41m[m
[32m+[m	[32m    int direction1 =  1;[m[41m[m
[32m+[m	[32m    int direction2 =  -1;[m[41m[m
[32m+[m	[32m    int potencia1=0;[m[41m[m
[32m+[m	[32m    int potencia2=0;[m[41m[m
[32m+[m	[32m    if(direccion == 1){[m[41m[m
[32m+[m	[41m    [m	[32mpotencia1=75;[m[41m[m
[32m+[m	[41m    [m	[32mpotencia2=75;[m[41m[m
[32m+[m	[41m    [m	[32mdirection1=-1;[m[41m[m
[32m+[m	[41m    [m	[32mdirection2=-1;[m[41m[m
[32m+[m	[32m    }else if(direccion == 2){[m[41m[m
[32m+[m	[41m    [m	[32mpotencia1=15;[m[41m[m
[32m+[m	[41m    [m	[32mpotencia2=55;[m[41m[m
[32m+[m	[41m    [m	[32mdirection1=-1;[m[41m[m
[32m+[m	[41m    [m	[32mdirection2=-1;[m[41m[m
[32m+[m	[32m    }else if(direccion == 3){[m[41m[m
[32m+[m	[41m    [m	[32mpotencia1=75;[m[41m[m
[32m+[m	[41m    [m	[32mpotencia2=75;[m[41m[m
[32m+[m	[41m    [m	[32mdirection1=1;[m[41m[m
[32m+[m	[41m    [m	[32mdirection2=1;[m[41m[m
[32m+[m	[32m    }else if(direccion == 4){[m[41m[m
[32m+[m	[41m    [m	[32mpotencia1=55;[m[41m[m
[32m+[m	[41m    [m	[32mpotencia2=15;[m[41m[m
[32m+[m	[41m    [m	[32mdirection1=-1;[m[41m[m
[32m+[m	[41m    [m	[32mdirection2=-1;[m[41m[m
[32m+[m	[32m    }[m[41m[m
[32m+[m	[32m    sendBTCmessage(BTCommunicator.NO_DELAY, motorActionb, potencia1*direction1*directionAction, 0);[m[41m[m
[32m+[m	[32m    sendBTCmessage(1000, motorActionb, 0, 0);[m[41m[m
[32m+[m		[32msendBTCmessage(BTCommunicator.NO_DELAY, motorActionc, potencia2*direction2*directionAction, 0);[m[41m[m
[32m+[m		[32msendBTCmessage(1000, motorActionc, 0, 0);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m}*/[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    private int byteToInt(byte byteValue) {[m[41m[m
[32m+[m[32m        int intValue = (byteValue & (byte) 0x7f);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if ((byteValue & (byte) 0x80) != 0)[m[41m[m
[32m+[m[32m            intValue |= 0x80;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return intValue;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    //retorna si esta pareado[m[41m [m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    public boolean isPairing() {[m[41m[m
[32m+[m[32m        return pairing;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    public boolean isConnected(){[m[41m[m
[32m+[m[41m    [m	[32mreturn connected;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    //recibe datos de dispositivo e inicia la conexion[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    public void onActivityResult(int requestCode, int resultCode, Intent data) {[m[41m[m
[32m+[m[32m        switch (requestCode) {[m[41m[m
[32m+[m[32m            case REQUEST_CONNECT_DEVICE:[m[41m[m
[32m+[m[41m[m
[32m+[m[41m                [m
[32m+[m[32m                if (resultCode == Activity.RESULT_OK) {[m[41m[m
[32m+[m[32m                    String address = data.getExtras().getString(DeviceListActivity.EXTRA_DEVICE_ADDRESS);[m[41m[m
[32m+[m[32m                    pairing = data.getExtras().getBoolean(DeviceListActivity.PAIRING);[m[41m[m
[32m+[m[32m                    startBTCommunicator(address);[m[41m[m
[32m+[m[41m                    [m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m                [m
[32m+[m[32m                break;[m[41m[m
[32m+[m[41m                [m
[32m+[m[32m            case REQUEST_ENABLE_BT:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // When the request to enable Bluetooth returns[m[41m[m
[32m+[m[32m                switch (resultCode) {[m[41m[m
[32m+[m[32m                    case Activity.RESULT_OK:[m[41m[m
[32m+[m[32m                        btOnByUs = true;[m[41m[m
[32m+[m[32m                        selectTypeCnt();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case Activity.RESULT_CANCELED:[m[41m[m
[32m+[m[32m                        showToast(R.string.bt_needs_to_be_enabled, Toast.LENGTH_SHORT);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        showToast(R.string.problem_at_connecting, Toast.LENGTH_SHORT);[m[41m[m
[32m+[m[32m                        finish();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m                [m
[32m+[m[32m                break;[m[41m[m
[32m+[m[41m             [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    //administra los errores que pueden suceder antes y durante la conexion.[m[41m[m
[32m+[m[32m    final Handler myHandler = new Handler() {[m[41m[m
[32m+[m[32m        @Override[m[41m[m
[32m+[m[32m        public void handleMessage(Message myMessage) {[m[41m[m
[32m+[m[32m            switch (myMessage.getData().getInt("message")) {[m[41m[m
[32m+[m[32m                case BTCommunicator.DISPLAY_TOAST:[m[41m[m
[32m+[m[32m                    showToast(myMessage.getData().getString("toastText"), Toast.LENGTH_SHORT);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case BTCommunicator.STATE_CONNECTED:[m[41m[m
[32m+[m[32m                    connected = true;[m[41m[m
[32m+[m[32m                    connectingProgressDialog.dismiss();[m[41m[m
[32m+[m[32m                    sendBTCmessage(BTCommunicator.NO_DELAY, BTCommunicator.GET_FIRMWARE_VERSION, 0, 0);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[41m                    [m
[32m+[m[32m                case BTCommunicator.MOTOR_STATE:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (myBTCommunicator != null) {[m[41m[m
[32m+[m[32m                        byte[] motorMessage = myBTCommunicator.getReturnMessage();[m[41m[m
[32m+[m[32m                        int position = byteToInt(motorMessage[21]) + (byteToInt(motorMessage[22]) << 8) + (byteToInt(motorMessage[23]) << 16)[m[41m[m
[32m+[m[32m                                       + (byteToInt(motorMessage[24]) << 24);[m[41m[m
[32m+[m[32m                        showToast(getResources().getString(R.string.current_position) + position, Toast.LENGTH_SHORT);[m[41m[m

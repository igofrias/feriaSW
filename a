[31m-                	stopProgram();[m
[31m-                    break;[m
[31m-                case GET_PROGRAM_NAME:[m
[31m-                	Log.d("pepe", "got name  >:C");[m
[31m-                    getProgramName();[m
[31m-                    Log.d("pepe", "got name  >:C fin");[m

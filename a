[32m+[m[32mpublic class EatTask implements SensorEventListener, Runnable {[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mSensorManager manager;[m[41m[m
[32m+[m	[32mSensor accelerometer;[m[41m[m
[32m+[m	[32mdouble[] gravity;[m[41m[m
[32m+[m	[32mdouble mAccelLast;[m[41m[m
[32m+[m	[32mdouble mAccelCurrent;[m[41m[m
[32m+[m	[32mdouble mAccel;[m[41m[m
[32m+[m	[32mBoolean action;[m[41m[m
[32m+[m	[32mstatic double ACCEL_MIN = 3;[m[41m[m
[32m+[m	[32mstatic long MIN_TIME = 1000; //en milisegundos[m[41m[m
[32m+[m	[32mstatic boolean running;[m[41m[m
[32m+[m	[32mActivity parent;[m[41m[m
[32m+[m	[32mEatTask thisTask = this;[m[41m[m
[32m+[m	[32m//Handler ext_handler;[m[41m[m
[32m+[m	[32mPetActionManager pet_manager;[m[41m[m
[32m+[m	[32m  public EatTask(Activity activity, PetActionManager petman) {[m[41m[m
[32m+[m		[32m  //Sets view and sensor[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mparent = activity;[m[41m[m
[32m+[m		[32mpet_manager = petman;[m[41m[m
[32m+[m	[32m    manager =(SensorManager) parent.getSystemService(Context.SENSOR_SERVICE);[m[41m[m
[32m+[m		[32maccelerometer = manager.getDefaultSensor(Sensor.TYPE_ACCELEROMETER);[m[41m[m
[32m+[m		[32mgravity = new double[3];[m[41m[m
[32m+[m		[32maction = false;[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m    return;[m[41m[m
[32m+[m	[32m  }[m[41m[m
[32m+[m	[32m  CountDownTimer timer = new CountDownTimer(MIN_TIME, MIN_TIME/100)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m@Override[m[41m[m
[32m+[m			[32mpublic void onFinish() {[m[41m[m
[32m+[m				[32m//Hace la accion apenas termina[m[41m[m
[32m+[m				[32mdoTaskAction();[m[41m[m
[32m+[m				[32mLog.d("EatTask","End of task");[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m			[32m@Override[m[41m[m
[32m+[m			[32mpublic void onTick(long millisUntilFinished) {[m[41m[m
[32m+[m				[32m//Revisa que la accion se haya ejecutado dentro de este tick.[m[41m[m
[32m+[m				[32mLog.d("EatTask","It's Ticking");[m[41m[m
[32m+[m[41m				[m
[32m+[m			[32m}[m[41m[m
[32m+[m[41m			[m
[32m+[m		[32m};[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic void onAccuracyChanged(Sensor arg0, int arg1) {[m[41m[m
[32m+[m		[32m// TODO Auto-generated method stub[m[41m[m
[32m+[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m	[32mpublic double setGravity(double raw_accel_x, double raw_accel_y,[m[41m [m
[32m+[m			[32mdouble raw_accel_z)[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Calcula la gravedad para despues quitar su influencia[m[41m[m
[32m+[m		[32m//alpha  = t/(dt + t) t=constante del filtro dt=event delivery rate[m[41m[m
[32m+[m		[32mdouble alpha = 0.8;[m[41m[m
[32m+[m		[32mfor (int i = 0; i < 3; i++)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mgravity[i] = 0;[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32m  // Isolate the force of gravity with the low-pass filter.[m[41m[m
[32m+[m		[32m  gravity[0] = alpha * gravity[0] + (1 - alpha) * raw_accel_x;[m[41m[m
[32m+[m		[32m  gravity[1] = alpha * gravity[1] + (1 - alpha) * raw_accel_y;[m[41m[m
[32m+[m		[32m  gravity[2] = alpha * gravity[2] + (1 - alpha) * raw_accel_z;[m[41m[m
[32m+[m[41m		  [m
[32m+[m		[32m  double x = raw_accel_x - gravity[0];[m[41m[m
[32m+[m		[32m  double y = raw_accel_y - gravity[1];[m[41m[m
[32m+[m		[32m  double z = raw_accel_z - gravity[2];[m[41m[m
[32m+[m		[32mdouble gravitymod = Math.sqrt(x*x + y*y);[m[41m[m
[32m+[m		[32mreturn gravitymod;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic void onSensorChanged(SensorEvent arg0) {[m[41m[m
[32m+[m		[32m// Gets information from sensors and processes it[m[41m[m
[32m+[m		[32mfloat x = arg0.values[0];[m[41m[m
[32m+[m	[32m      float y = arg0.values[1];[m[41m[m
[32m+[m	[32m      float z = arg0.values[2];[m[41m[m
[32m+[m	[32m      mAccelLast = mAccelCurrent;[m[41m[m
[32m+[m	[32m      mAccelCurrent = setGravity(x,y,z);[m[41m [m
[32m+[m	[32m      double delta = mAccelCurrent - mAccelLast;[m[41m[m
[32m+[m	[32m      mAccel = mAccel * 0.9f + delta*0.1f; // perform low-cut filter[m[41m[m
[32m+[m	[32m      //After shaking does the action and ends[m[41m[m
[32m+[m	[32m      if (delta > ACCEL_MIN)[m[41m[m
[32m+[m	[32m      {[m[41m[m
[32m+[m	[41m    [m	[32m  Log.d("EatTask","Accion ejecutada");[m[41m[m
[32m+[m	[41m    [m	[32m  action = true;[m[41m[m
[32m+[m	[41m    [m	[32m  timer.cancel();[m[41m[m
[32m+[m	[41m    [m	[32m  doTaskAction();[m[41m[m
[32m+[m	[41m    [m	[32m  //para que vibre al realizar accion[m[41m[m
[32m+[m	[41m    [m	[32m  Vibrator vibe = (Vibrator) parent.getSystemService(Context.VIBRATOR_SERVICE);[m[41m	[m
[32m+[m	[41m    [m	[32m  vibe.vibrate(100);[m[41m [m
[32m+[m	[41m    [m	[32m  Log.d("EatTask","End of task - Action");[m[41m[m
[32m+[m[41m				[m
[32m+[m[41m	    	  [m
[32m+[m	[32m      }[m[41m[m
[32m+[m	[32m      else[m[41m [m
[32m+[m	[32m      {[m[41m[m
[32m+[m	[41m    [m	[32m  action = false;[m[41m[m
[32m+[m	[32m      }[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m[41m	[m
[32m+[m	[32m@Override[m[41m[m
[32m+[m	[32mpublic void run() {[m[41m[m
[32m+[m		[32m// TODO Auto-generated method stub[m[41m[m
[32m+[m[41m		[m
[32m+[m		[32mmanager.registerListener(thisTask, accelerometer, SensorManager.SENSOR_DELAY_NORMAL);[m[41m[m
[32m+[m		[32mtimer.start();[m[41m[m
[32m+[m		[32mwhile(running)[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mif(!running || action)[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m				[32mbreak;[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32mthis.doTaskAction();[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic boolean actionDone(){[m[41m[m
[32m+[m		[32m//Retorna si ha sido ejecutada la accion[m[41m[m
[32m+[m		[32mreturn action;[m[41m[m
[32m+[m	[32m}[m[41m[m
[32m+[m	[32mpublic void doTaskAction()[m[41m[m
[32m+[m	[32m{[m[41m[m
[32m+[m		[32m//Accion que debe ejecutar la task, incluido detener todo;[m[41m[m
[32m+[m		[32mif (this.actionDone())[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mToast.makeText(parent.getBaseContext(), "Comio", Toast.LENGTH_SHORT).show();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32melse[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mToast.makeText(parent.getBaseContext(), "No hay acciones disponibles", Toast.LENGTH_SHORT).show();[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m		[32maction = false;[m[41m[m
[32m+[m		[32mmanager.unregisterListener(thisTask);[m[41m[m
[32m+[m		[32mpet_manager.stop_everything();[m[41m[m
[32m+[m[41m		[m
[32m+[m	[32m}[m[41m[m
[32m+[m[32m}[m[41m[m
